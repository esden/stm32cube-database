            <Output signalId="SYSCLKOUT" to="MCOMult"/>
			<Output signalId="SYSCLKOUT" to="PWROutput"/>
        </Element>
		
		<Element id="PWROutput" type="activeOutput" refParameter="PWRFreq_Value" x="1103" y="195">
