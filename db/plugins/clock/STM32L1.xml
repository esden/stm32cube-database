            <Output signalId="TimCLK" to="TimPrescOut"/>
        </Element>
		
		
        <Element id="TimPrescOut" type="activeOutput" refParameter="APB1TimFreq_Value" x="1102" y="445">
            <Input signalId="TimCLK" from="TimPrescalerAPB1"/>
