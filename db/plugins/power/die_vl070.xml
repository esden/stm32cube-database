        <Package Names="LQFP64" ThermalResistance="44"/>
        <Package Names="LQFP48" ThermalResistance="55"/>
        <Package Names="TSSOP20" ThermalResistance="76"/>
    </Packages>
    <DMIPS FlashValueByMHz="0" RamValueByMhz="0"/>
