                <PowerModes PowerModesList="RUN,SLEEP,STOP">
                    <Consumption GlobalConsumption="0"/>
                </PowerModes>
            </PeripheralConsumption>
        </Peripheral>
    </Peripherals>
    <PeripheralsLists>
        <PowerMode Name="RUN">
            <Filter IpsFromList="Include"/>
        </PowerMode>
        <PowerMode Name="SLEEP">
            <Filter IpsFromList="Include"/>
        </PowerMode>
        <PowerMode Name="STOP">
            <Filter IpsFromList="Include"/>
        </PowerMode>
        <PowerMode Name="STANDBY">
            <Filter IpsFromList="Include"/>
        </PowerMode>
    </PeripheralsLists>
</Die>