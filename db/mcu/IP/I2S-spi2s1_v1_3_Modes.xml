<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<IP DBVersion="V3.0" IPType="peripheral" Name="I2S" Version="spi2s1_v1_3" xmlns:ns0="http://www.w3.org/2001/XMLSchema-instance" ns0:schemaLocation="http://mcd.rou.st.com/modules.php?name=mcu ../../db/IP_Modes_V3_0.xsd" xmlns="http://mcd.rou.st.com/modules.php?name=mcu">
	<ModeLogicOperator Name="OR">
		<Mode Name="Mode">
			<ModeLogicOperator Name="XOR">
				<Mode Name="Master">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Master-With-Clock">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="MCK"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Slave">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>$IpInstance</Semaphore>
			<Condition Expression="!SPI$IpNumber" Diagnostic="SPI$IpNumber must be disabled"/>
		</Mode>
	</ModeLogicOperator>
	<RefSignal IOMode="GPIO" Name="CK"/>
	<RefSignal IOMode="GPIO" Name="MCK"/>
	<RefSignal IOMode="GPIO" Name="SD"/>
	<RefSignal IOMode="GPIO" Name="WS"/>
</IP>
