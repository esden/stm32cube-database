        <PossibleValue Comment="1" Value="DAC_TRIANGLEAMPLITUDE_1"/>
        <PossibleValue Comment="3" Value="DAC_TRIANGLEAMPLITUDE_3"/>
        <PossibleValue Comment="7" Value="DAC_TRIANGLEAMPLITUDE_7"/>
