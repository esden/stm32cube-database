<!--=============================== Memories Protection ===================================-->
	<!--SYSRAM-->
  <RefParameter Name="ConfigureSYSRAM" Comment="Configure Memory" DefaultValue="Secure" Type="string"></RefParameter>
  <RefParameter Name="SYSRAM_start_adress" Comment="Start Adress" DefaultValue="0x2FFE0000" Type="string" Min="0x2FFE0000" Max="0x30000000">
  </RefParameter>
  <RefParameter Name="SYSRAMsize" Comment="Size" DefaultValue="0x1000" Type="hex" Min="0x0" Max="0x20000">
    <Description>must be multiple of 0x1000</Description>
  </RefParameter>
  <RefParameter Name="SYSRAM_lock" Comment="Loking SYSRAM" DefaultValue="Unlock" Type="list">
    <PossibleValue Value="Unlock" Comment="Unlock"/>
    <PossibleValue Value="Lock" Comment="Lock" Semaphore="SYSRAM_lock"/>
  </RefParameter>
	<!--ROM-->
  <RefParameter Name="ConfigureROM" Comment="Configure Memory" DefaultValue="Secure" Type="string"></RefParameter>
  <RefParameter Name="ROM_start_adress" Comment="Start Adress" DefaultValue="0x00000000" Type="string" Min="0x00000000" Max="0x00020000">
  </RefParameter>
  <RefParameter Name="ROMsize" Comment="Size" DefaultValue="0x1000" Type="hex" Min="0x0" Max="0x20000">
    <Description>must be multiple of 0x1000</Description>
  </RefParameter>
  <RefParameter Name="ROM_lock" Comment="Locking ROM" DefaultValue="Unlock" Type="list">
    <PossibleValue Value="Unlock" Comment="Unlock"/>
    <PossibleValue Value="Lock" Comment="Lock"/>
  </RefParameter>

