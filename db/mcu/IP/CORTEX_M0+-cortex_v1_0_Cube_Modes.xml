        <PossibleValue Value="MPU_ACCESS_NOT_BUFFERABLE" Comment="DISABLE"/>
        <PossibleValue Value="MPU_ACCESS_BUFFERABLE" Comment="ENABLE"/>	 
        <Description>Specifies the bufferable status of the protected region</Description> 	
    </RefParameter>	
		
