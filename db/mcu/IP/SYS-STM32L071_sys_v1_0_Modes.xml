	<!-- Modes for Debug (JTAG and Trace) -->
	<ModeLogicOperator Name="OR">
		<Mode Name="Debug" UserName="Debug Serial Wire">
			<ModeLogicOperator Name="XOR">
				<Mode Name="Serial_Wire">
					<SignalLogicalOp Name="AND">
						<Signal Name="SWCLK"/>
						<Signal Name="SWDIO"/>
					</SignalLogicalOp>
					<Semaphore>SWD</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		
		<!-- Modes for System Wake-Up -->
		<Mode Name="SYS_WakeUp0" UserName="System Wake-Up 1">
			<ModeLogicOperator Name="OR">
				<Mode Name="SYS_WakeUp0">
					<SignalLogicalOp Name="AND">
						<Signal Name="WKUP1"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		
		<Mode Name="SYS_WakeUp1"  UserName="System Wake-Up 2">
			<ModeLogicOperator Name="OR">
				<Mode Name="SYS_WakeUp1">
					<SignalLogicalOp Name="AND">
						<Signal Name="WKUP2"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<Mode Name="SYS_WakeUp2"  UserName="System Wake-Up 3">
			<ModeLogicOperator Name="OR">
				<Mode Name="SYS_WakeUp2">
					<SignalLogicalOp Name="AND">
						<Signal Name="WKUP3"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<!-- ADC VEREFINT Connection PAD-->
		<Mode Name="VREFINT_ADC"  UserName="VREFINT_ADC Connection">
			<ModeLogicOperator Name="XOR">
				<Mode Name="VREFINT_ADC_PB0" UserName="PB0 connected">
					<SignalLogicalOp Name="AND">
						<Signal Name="VREF_OUT_PB0"/>
						
					</SignalLogicalOp>
					<Semaphore>VREFINT_OUT_PB0_USED</Semaphore>
				</Mode>
				<Mode Name="VREFINT_ADC_PB1" UserName="PB1 connected">
					<SignalLogicalOp Name="AND">
						<Signal Name="VREF_OUT_PB1"/>
						
					</SignalLogicalOp>
					<Semaphore>VREFINT_OUT_PB1_USED</Semaphore>
				</Mode>
				<Mode Name="VREFINT_ADC_PB0_PB1" UserName="PB0 and PB1 connected">
					<SignalLogicalOp Name="AND">
						<Signal Name="VREF_OUT_PB0"/>
						<Signal Name="VREF_OUT_PB1"/>						
					</SignalLogicalOp>
					<Semaphore>VREFINT_OUT_PB0_PB1_USED</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<!-- Programmable Voltage Detector-->
		<Mode Name="PowerVD" UserName="Power Voltage Detector In" RemoveCondition="STM32L0x0_Value_Line">
			<ModeLogicOperator Name="XOR">
				<Mode Name="Mode_PVD_Ext"
					UserName="Power Voltage Detector In (External input analog voltage)">
					<SignalLogicalOp Name="AND">
						<Signal Name="PVD_IN" />
					</SignalLogicalOp>
					<Semaphore>Sem_ExternalAnalogVoltage</Semaphore>
					<Semaphore>PVD_INTERRUPT</Semaphore>

				</Mode>
				<Mode Name="Mode_PVD_Int"
					UserName="Power Voltage Detector In (Internal analog voltage)">
					<SignalLogicalOp Name="AND">
						<Signal Name="V_PVD_IN" />
					</SignalLogicalOp>
					<Semaphore>PVD_INTERRUPT</Semaphore>

				</Mode>
			</ModeLogicOperator>
		</Mode>
