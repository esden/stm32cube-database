                      </SignalLogicalOp>
                  </Mode>
              </ModeLogicOperator>
              <Semaphore>S_Crypt_Enabled</Semaphore>
              </Mode>
      </ModeLogicOperator>
    
    <!-- RefSignal for Crypt -->
    <RefSignal Name="VS_Crypt" Virtual="true"/>

