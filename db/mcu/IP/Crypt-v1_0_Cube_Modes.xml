  <About>Crypt </About>
	
    <!-- Generic RefParameters for Crypt --> 
    <RefParameter Name="Instance" DefaultValue="Crypt" Comment="Instance" Type="" Visible="false"/>

    <RefMode Name="Crypt_Activate" HalMode ="Crypt">
        <ConfigForMode>Crypt_Init</ConfigForMode>
