						<Signal Name="TRACED0" />
						<Signal Name="TRACED1" />
					</SignalLogicalOp>
					<Semaphore>SWD</Semaphore>
				</Mode>

				<Mode Name="Trace_Synchro_4bits_SW" UserName="Trace Synchro Sw (4 bits)">
					<SignalLogicalOp Name="AND">
						<Signal Name="JTMS-SWDIO" />
						<Signal Name="JTCK-SWCLK" />
