						<Signal Name="$SingleInstanceVREFBUF_OUT" />
					</SignalLogicalOp>
					<Semaphore>Sem_InternalMode</Semaphore>
				</Mode>				
				<Mode Name="HoldMode" UserName="Hold">
					<SignalLogicalOp Name="AND">
						<Signal Name="V_VREFBUF" />
					</SignalLogicalOp>
					<Semaphore>Sem_HoldMode</Semaphore>
				</Mode>					
			</ModeLogicOperator>
		</Mode>	
                
