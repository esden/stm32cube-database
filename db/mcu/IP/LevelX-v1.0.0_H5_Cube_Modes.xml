	<RefParameter Added="true" Comment="LX_DIRECT_READ" DefaultValue="0" Group="NOR Flash Support" Name="LX_DIRECT_READ" TabName="LevelX" Type="list">
        <Description>Defined, this option bypasses the NOR flash driver read routine in favor or reading the NOR memory directly, resulting in a significant performance increase.</Description>
        <PossibleValue Comment="Disabled" Value="0"/>
        <PossibleValue Comment="Enabled" Value="1"/>
    </RefParameter>
    <RefParameter Added="true" Comment="LX_FREE_SECTOR_DATA_VERIFY" DefaultValue="0" Group="NOR Flash Support" Name="LX_FREE_SECTOR_DATA_VERIFY" TabName="LevelX" Type="list">
        <Description>Defined, this causes the LevelX NOR instance open logic to verify free NOR sectors are all ones.</Description>
        <PossibleValue Comment="Disabled" Value="0"/>
        <PossibleValue Comment="Enabled" Value="1"/>
    </RefParameter>
    <RefParameter Added="true" Comment="LX_NOR_DISABLE_EXTENDED_CACHE" DefaultValue="0" Group="NOR Flash Support" Name="LX_NOR_DISABLE_EXTENDED_CACHE" TabName="LevelX" Type="list">
        <Description>Defined, this disables the extended NOR cache.</Description>
        <PossibleValue Comment="Disabled" Value="0"/>
        <PossibleValue Comment="Enabled" Value="1"/>
    </RefParameter>
	<RefParameter Added="true" Comment="LX_NOR_EXTENDED_CACHE_SIZE" DefaultValue="8" Group="NOR Flash Support" Max="8" Min="0" Name="LX_NOR_EXTENDED_CACHE_SIZE" TabName="LevelX" Type="integer" Unit="">
        <Description>Represent the number of sectors that can be cashed in a NOR instance.</Description>
    </RefParameter>
	<RefParameter Added="true" Comment="LX_NOR_SECTOR_MAPPING_CACHE_SIZE" DefaultValue="=INTPOW(2,NOR_SECTOR_MAPPING_CACHE_SIZE_NB_BIT)" Group="NOR Flash Support" Name="LX_NOR_SECTOR_MAPPING_CACHE_SIZE" TabName="LevelX" Type="string">
        <Description>By default this value is 16 and defines the logical sector mapping cache size. Large values improve performance, but cost memory. 
The minimum size is 8 and all values must be a power of 2.</Description>
    </RefParameter>
	<RefParameter Added="true" Comment="NOR_SECTOR_MAPPING_CACHE_SIZE_NB_BIT" DefaultValue="4" Group="NOR Flash Support" Max="10" Min="3" Name="NOR_SECTOR_MAPPING_CACHE_SIZE_NB_BIT" TabName="LevelX" Type="integer" Unit="">
        <Description>represents the power of which LX_NOR_SECTOR_MAPPING_CACHE_SIZE is rised to</Description>
    </RefParameter>
	
	<RefParameter Added="true" Comment="LX_THREAD_SAFE_ENABLE" DefaultValue="0" Group="Core" Name="LX_THREAD_SAFE_ENABLE" TabName="LevelX" Type="list">
