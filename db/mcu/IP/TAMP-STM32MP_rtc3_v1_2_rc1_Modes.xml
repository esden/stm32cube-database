<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<IP DBVersion="V4.0" IPType="peripheral" Name="TAMP" Version="STM32MP_rtc3_v1_2_rc1"
	xmlns:ns0="http://www.w3.org/2001/XMLSchema-instance"
	ns0:schemaLocation="http://mcd.rou.st.com/modules.php?name=mcu ../../../../../../../doc/V4/Development/Specifications/db/IP_Modes.xsd"
	xmlns="http://mcd.rou.st.com/modules.php?name=mcu">
	<About>TAMP: Tamp</About>

	<!--  -->
      
	<ModeLogicOperator Name="OR">
           <Mode Name="Activated" RemoveDisable="true">
			<ModeLogicOperator Name="XOR">
				<Mode Name="TAMP_Enabled" >
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_TAMP_Activate"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
                         
		</Mode>
		
		<Mode Name="TAMP_IN1">
			<ModeLogicOperator Name="XOR">
				<Mode Name="TAMP_IN1">
					<SignalLogicalOp Name="AND">
						<Signal Name="IN1" />
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<Mode Name="TAMP_IN2">
			<ModeLogicOperator Name="XOR">
				<Mode Name="TAMP_IN2">
					<SignalLogicalOp Name="AND">
						<Signal Name="IN2" />
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<Mode Name="TAMP_IN3">
			<ModeLogicOperator Name="XOR">
				<Mode Name="TAMP_IN3">
					<SignalLogicalOp Name="AND">
						<Signal Name="IN3" />
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<Mode Name="TAMP_OUT2">
			<ModeLogicOperator Name="XOR">
				<Mode Name="TAMP_OUT2">
					<SignalLogicalOp Name="AND">
						<Signal Name="OUT2" />
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<Mode Name="TAMP_OUT3">
			<ModeLogicOperator Name="XOR">
				<Mode Name="TAMP_OUT3">
					<SignalLogicalOp Name="AND">
						<Signal Name="OUT3" />
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
	</ModeLogicOperator>


	<!-- RefSignal for System -->
	<RefSignal IOMode="System" Name="IN1" />
        <RefSignal IOMode="System" Name="IN2" />
        <RefSignal IOMode="System" Name="IN3" />
        <RefSignal IOMode="System" Name="OUT2" />
        <RefSignal IOMode="System" Name="OUT3" />
        <RefSignal Name="VS_TAMP_Activate" Virtual="true"/>



</IP>
