                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_OUT2_Int</Semaphore>
                    <Semaphore>$IpInstance_OUT2_Conf</Semaphore>
                </Mode>
                <Mode Name="DAC_OUT2_ExtAndInt" UserName="both to external pin and to on chip analog peripherals">
