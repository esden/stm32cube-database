        <Parameter Name="SrcDataWidth_OUTPUTLEVEL_DATA"/>
        <Parameter Name="DestDataWidth_OUTPUTLEVEL_DATA"/>
        <Parameter Name="TransferEventMode_OUTPUTLEVEL_DATA"/>
