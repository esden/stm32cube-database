<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<IP DBVersion="V3.0" IPType="peripheral" Name="ADC" Version="aditf_v2_5_Cube" xmlns:ns0="http://www.w3.org/2001/XMLSchema-instance" ns0:schemaLocation="http://mcd.rou.st.com/modules.php?name=mcu ../../db/IP_Modes_V3_0.xsd" xmlns="http://mcd.rou.st.com/modules.php?name=mcu">
    <About>ADC: Analog to Digital Converter</About>
	<!--
	<RefParameter Comment="Fadc Constraint" DefaultValue="14000000" Max="14000000" Min="600000" Name="FadcConstraint" Type="double" Visible="false"/>
	-->
	
	<RefParameter Comment="Master List" DefaultValue="" Name="MasterList" Type="list" Visible="false"/>
    <!-- ADC_Settings parameters -->    
    <RefMode Name="ADC_Settings" Comment="ADCx Settings" Group="ADC_Settings">
        <ConfigForMode>ADC_RegularConfig</ConfigForMode>
        <Parameter Name="DataAlign"/>
        <Parameter Name="ScanConvMode"/>
        <Parameter Name="ContinuousConvMode"/>
        <Parameter Name="DiscontinuousConvMode"/>
        <Parameter Name="NbrOfDiscConversion"/>
    </RefMode>
    
    <!-- ADC Instance parameter needed in ADC_HandleTypeDef structure in the HAL -->
    <RefParameter Comment="ADC instance" DefaultValue="ADC1" Name="Instance" Type="list" Group="ADCgroup" Visible="false"/>
        
    <!-- possible value of DataAlign parameter -->
    <RefParameter Comment="Data Alignment" DefaultValue="ADC_DATAALIGN_RIGHT" Name="DataAlign" Type="list" Group="ADCgroup">
        <PossibleValue Comment="Right alignment" Value="ADC_DATAALIGN_RIGHT"/>
        <PossibleValue Comment="Left alignment" Value="ADC_DATAALIGN_LEFT"/>
        <Description>Select the alignment of the data stored after conversion.</Description>
    </RefParameter>
	    
    <!-- ScanconvMode -->
    <!--RefParameter Comment="Scan Conversion Mode" DefaultValue="DISABLE" Name="ScanConvMode" Type="list" Group="ADCgroup">
        <PossibleValue Comment="Disabled" Value="DISABLE"/>
        <PossibleValue Comment="Enabled" Value="ENABLE"/>
        <Description>Enable or Disable the Scan Conversion Mode</Description>
    </RefParameter-->
    <RefParameter Comment="Scan Conversion Mode" DefaultValue="ADC_SCAN_ENABLE" Name="ScanConvMode" Type="list" Group="ADCgroup">
                <Condition Expression="(NbrOfConversion>1&amp;EnableRegularConversion=ENABLE)|(InjNumberOfConversion>1)" Diagnostic=""/>
                <PossibleValue Comment="Enabled" Value="ADC_SCAN_ENABLE"/>
            <Description>Enable or Disable the Scan Conversion Mode</Description>
        </RefParameter>
        <RefParameter Comment="Scan Conversion Mode" DefaultValue="ADC_SCAN_DISABLE" Name="ScanConvMode" Type="list" Group="ADCgroup">
            <PossibleValue Comment="Disabled" Value="ADC_SCAN_DISABLE"/>
            <Description>Enable or Disable the Scan Conversion Mode</Description>
        </RefParameter>
    <!-- Possible values of ContinuousConvMode parameter -->
    <RefParameter Comment="Continuous Conversion Mode" DefaultValue="DISABLE" Name="ContinuousConvMode" Type="list" Group="ADCgroup">
        <Condition Expression="DiscontinuousConvMode=ENABLE" Diagnostic="Continuous and Discontinuous modes could not be used at the same time"/>
        <PossibleValue Comment="Disabled" Value="DISABLE"/>
    </RefParameter>
    <RefParameter Comment="Continuous Conversion Mode" DefaultValue="DISABLE" Name="ContinuousConvMode" Type="list" Group="ADCgroup">
        <PossibleValue Comment="Disabled" Value="DISABLE"/>
        <PossibleValue Comment="Enabled" Value="ENABLE" Semaphore="ContinuousMode"/>
        <Description>Enable or Disable the Continuous Conversion Mode</Description>
    </RefParameter>
	    
    <!-- Possible values of DiscontinuousConvMode parameter -->
    <RefParameter Comment="Discontinuous Conversion Mode" DefaultValue="DISABLE" Name="DiscontinuousConvMode" Type="list">
        <Condition Expression="ContinuousMode_$IpInstance" Diagnostic="Continuous and Discontinuous modes could not be used at the same time"/>
        <PossibleValue Comment="Disabled" Value="DISABLE"/>
    </RefParameter>
    <!--RefParameter Comment="Discontinuous Conversion Mode" DefaultValue="DISABLE" Name="DiscontinuousConvMode" Type="uniqueElementList">
        <Condition Expression="InjectedConvMode=Discontinuous" Diagnostic="Discontinuous mode could not be used for regular and inected conversion at the same time"/>
        <PossibleValue Comment="Disabled" Value="DISABLE"/>
    </RefParameter-->
     <RefParameter Comment="Discontinuous Conversion Mode" DefaultValue="DISABLE" Name="DiscontinuousConvMode" Type="list">
        <Condition Diagnostic="Discontinuous mode is used only if Scan Mode is enabled" Expression="ScanConvMode=ADC_SCAN_DISABLE"/>
        <PossibleValue Comment="Disabled" Value="DISABLE"/>
    </RefParameter>
    <RefParameter Comment="Discontinuous Conversion Mode" DefaultValue="DISABLE" Name="DiscontinuousConvMode" Type="list">
        <PossibleValue Comment="Disabled" Value="DISABLE"/>
        <PossibleValue Comment="Enabled" Value="ENABLE"/>
        <Description>Enable or Disable the Discontinuous Conversion Mode</Description>
    </RefParameter>
	    
    <!-- Possible values of NbrOfDiscConversion parameter -->
     <RefParameter Comment="Number Of Discontinuous Conversions" DefaultValue="null" Max="1" Min="1" Name="NbrOfDiscConversion" Type="integer" Visible="false">
    	<Condition Expression="(DiscontinuousConvMode=DISABLE)" Diagnostic=""/>
    </RefParameter>
    <RefParameter Comment="Number Of Discontinuous Conversions" DefaultValue="1" Max="8" Min="1" Name="NbrOfDiscConversion" Type="integer"/>
	    
        
    <!-- RefMode of ADC_Regular_ConversionMode -->
    <RefMode Name="ADC_Regular_ConversionMode" Comment="Regular Conversion Settings" Group="ADC_Regular_ConversionMode" Abstract="true">
        <Parameter Name="EnableRegularConversion"/>
        <Parameter Name="NbrOfConversionFlag"/>
        <Parameter Name="NbrOfConversion"/>
        <Parameter Name="ExternalTrigConv"/>
    </RefMode>
    <RefParameter Comment="Enable Regular Conversions" DefaultValue="ENABLE" Name="EnableRegularConversion" Type="list" Group="ADC_Regular_ConversionMode">
        <PossibleValue Comment="Enable" Value="ENABLE"/>
        <PossibleValue Comment="Disable" Value="DISABLE" />
        <Description>Enable/Disable Regular Conversion</Description> 
    </RefParameter>
    <!-- Possible values of NbrOfConversionFlag parameter  !!!! what is the utility of this? !!!!-->
    <RefParameter Comment="Number Of Conversion Flag" DefaultValue="0" Max="1" Min="0" Name="NbrOfConversionFlag" Type="integer" Group="ADCgroup" Visible="false"/>
        
    <!-- Possible values of NbrOfConversion parameter !!!!! the Group is not the same !!!!!! -->
    <RefParameter Comment="Number Of Conversion" DefaultValue="1" Max="1" Min="1" Name="NbrOfConversion" Type="integer" Group="ADCgroup" Visible="false">
        <Condition Expression="EnableRegularConversion=DISABLE" Diagnostic=""/>
        <Description>Number of regular channels to be converted</Description>
    </RefParameter>
    <RefParameter Comment="Number Of Conversion" DefaultValue="1" Max="16" Min="1" Name="NbrOfConversion" Type="integer" Group="ADCgroup">
        <Description>Number of regular channels to be converted</Description>
    </RefParameter>
        
	    
    <!-- ExternalTrigConv -->
    <RefParameter Comment="External Trigger Conversion Source" DefaultValue="null" Name="ExternalTrigConv" Type="list" Group="ADCgroup" Visible="false">
        <Condition Expression="EnableRegularConversion=DISABLE" Diagnostic=""/>
    </RefParameter>
    <RefParameter Comment="External Trigger Conversion Source" DefaultValue="ADC_EXTERNALTRIGCONV_Ext_IT11" Name="ExternalTrigConv" Type="list" Group="ADCgroup">
        <Condition Expression="(RegularExternalTrigger)" Diagnostic="EXTI Line11 is used for External-Trigger-for-Regular-conversion mode."/>
        <PossibleValue Comment="EXTI Line11" Value="ADC_EXTERNALTRIGCONV_EXT_IT11"/>
    </RefParameter>
    <RefParameter Comment="External Trigger Conversion Source" DefaultValue="ADC_SOFTWARE_START" Name="ExternalTrigConv" Type="list" Group="ADCgroup">
        <PossibleValue Comment="Regular Conversion launched by software" Value="ADC_SOFTWARE_START"/>       
        <PossibleValue Comment="Timer 19 Trigger Out event" Value="ADC_EXTERNALTRIGCONV_T19_TRGO"/>
        <PossibleValue Comment="Timer 19 Capture Compare 3 event" Value="ADC_EXTERNALTRIGCONV_T19_CC3"/>
        <PossibleValue Comment="Timer 19 Capture Compare 4 event" Value="ADC_EXTERNALTRIGCONV_T19_CC4"/>
        <PossibleValue Comment="Timer 2 Capture Compare 2 event" Value="ADC_EXTERNALTRIGCONV_T2_CC2"/>
        <PossibleValue Comment="Timer 3 Trigger Out event" Value="ADC_EXTERNALTRIGCONV_T3_TRGO"/>
        <PossibleValue Comment="Timer 4 Capture Compare 4 event" Value="ADC_EXTERNALTRIGCONV_T4_CC4"/>
        <Description>Select the external event used to trigger the start of conversion of a regular group</Description>
    </RefParameter>
	
    <!-- Parameters of  ADC_Injected_ConversionMode Mode -->
    <RefMode Name="ADC_Injected_ConversionMode" Comment="Injected Conversion Settings" Group="ADC_Injected_ConversionMode" Abstract="true">
        <Parameter Name="InjNumberOfConversion"/>
        <Parameter Name="ExternalTrigInjecConv"/>
        <Parameter Name="InjectedConvMode"/>
        <Parameter Name="AutoInjectedConv"/>
        <Parameter Name="InjectedDiscontinuousConvMode"/>
    </RefMode>
        
    <!-- Possible Value of InjNumberOfConversion parameter -->
    <RefParameter Comment="Number Of Conversions" DefaultValue="0" Max="4" Min="0" Name="InjNumberOfConversion" Type="integer" Group="ADCgroup">
        <Description>Number of injected channels to be converted</Description>
    </RefParameter>
        
    <!-- Possible Value of ExternalTrigInjecConv parameter -->
    <RefParameter Comment="External Trigger Source" DefaultValue="null" Name="ExternalTrigInjecConv" Type="list" Group="ADCgroup" Visible="false">
        <Condition Expression="(InjNumberOfConversion=0)" Diagnostic=""/>
    </RefParameter>

    <RefParameter Comment="External Trigger Source" DefaultValue="ADC_EXTERNALTRIGINJECCONV_EXT_IT15" Name="ExternalTrigInjecConv" Type="list" Group="ADCgroup">
        <Condition Expression="!(AutoInjected_$IpInstance) &amp;(InjectedExternalTrigger)" Diagnostic=""/>
        <PossibleValue Comment="EXTI line15" Value="ADC_EXTERNALTRIGINJECCONV_EXT_IT15"/>
        <Description>EXTI Line15 is used for External-Trigger-for-Injected-conversion mode</Description>
    </RefParameter>
    <RefParameter Comment="External Trigger Source" DefaultValue="__NULL" Group="ADCgroup" Name="ExternalTrigInjecConv" Type="list" >
		<Condition Diagnostic="" Expression="(AutoInjected_$IpInstance)"/>
        <PossibleValue Comment="External Trigger on injected channels are disabled (Auto-injection mode selected)" Value="ADC_INJECTED_SOFTWARE_START"/>
	</RefParameter>
    <RefParameter Comment="External Trigger Source" DefaultValue="ADC_INJECTED_SOFTWARE_START" Name="ExternalTrigInjecConv" Type="list" Group="ADCgroup">
        <PossibleValue Comment="Injected Conversion launched by software" Value="ADC_INJECTED_SOFTWARE_START"/>
        <PossibleValue Comment="Timer 19 Capture Compare 1 event" Value="ADC_EXTERNALTRIGINJECCONV_T19_CC1"/>
        <PossibleValue Comment="Timer 19 Capture Compare 2 event" Value="ADC_EXTERNALTRIGINJECCONV_T19_CC2"/>
        <PossibleValue Comment="Timer 2 Trigger Out event" Value="ADC_EXTERNALTRIGINJECCONV_T2_TRGO"/>
        <PossibleValue Comment="Timer 2 Capture Compare 1 event" Value="ADC_EXTERNALTRIGINJECCONV_T2_CC1"/>
        <PossibleValue Comment="Timer 3 Capture Compare 4 event" Value="ADC_EXTERNALTRIGINJECCONV_T3_CC4"/>
        <PossibleValue Comment="Timer 4 Trigger Out event" Value="ADC_EXTERNALTRIGINJECCONV_T4_TRGO"/>
        <Description>Select the external event used to trigger the start of conversion of an injected group</Description>
    </RefParameter>
	    
    <!-- Possible Value of InjectedConvMode parameter -->
    <RefParameter Comment="Injected Conversion Mode" DefaultValue="null" Name="InjectedConvMode" Type="list" Visible="false">
        <Condition Expression="InjNumberOfConversion=0" Diagnostic=""/>
    </RefParameter>
    <!-- 
    After verification : it's possible to use Discontinuous mode a the same time in Reg/Inj conversions.-->
    <RefParameter Comment="Injected Conversion Mode" DefaultValue="None" Name="InjectedConvMode" Type="list">
        <Condition Expression="ScanConvMode=ADC_SCAN_DISABLE" Diagnostic="Discontinuous Mode is available only if Scan Conversion Mode is enabled"/>
        <PossibleValue Comment="None" Value="None"/>
        <PossibleValue Comment="Auto Injected Mode" Value="AutoInjected"  Semaphore="AutoInjected_$IpInstance"/>
    </RefParameter> 
    
    <RefParameter Comment="Injected Conversion Mode" DefaultValue="None" Name="InjectedConvMode" Type="list" Group="ADCgroup">
        <PossibleValue Comment="None" Value="None"/>
        <PossibleValue Comment="Discontinuous Mode" Value="Discontinuous" Semaphore="InjectedDiscontinuous_$IpInstance"/>
        <PossibleValue Comment="Auto Injected Mode" Value="AutoInjected"  Semaphore="AutoInjected_$IpInstance"/>
        <Description>Injected Conversion Mode selection</Description>
    </RefParameter>
    <RefParameter Comment="Injected Discontinuous Conversion Mode" DefaultValue="ENABLE" Name="InjectedDiscontinuousConvMode" Type="list" Visible="false">
        <Condition Expression="InjectedDiscontinuous_$IpInstance" Diagnostic=""/>
        <PossibleValue Comment="Enabled" Value="ENABLE"/>
    </RefParameter>
    <RefParameter Comment="Injected Discontinuous Conversion Mode" DefaultValue="DISABLE" Name="InjectedDiscontinuousConvMode" Type="list" Visible="false">
        <PossibleValue Comment="Disable" Value="DISABLE"/>
    </RefParameter>
    <RefParameter Comment="Automatic Injected Conversion" DefaultValue="ENABLE" Name="AutoInjectedConv" Type="list" Visible="false">
        <Condition Expression="AutoInjected_$IpInstance" Diagnostic=""/>
        <PossibleValue Comment="Enabled" Value="ENABLE"/>
    </RefParameter>
    <RefParameter Comment="Automatic Injected Conversion" DefaultValue="DISABLE" Name="AutoInjectedConv" Type="list" Visible="false">
        <PossibleValue Comment="Disable" Value="DISABLE"/>
    </RefParameter>
	    
    
    <!-- Parameters of ChannelRegularConversio Mode -->    
    <RefMode Name="ChannelRegularConversion" Comment="Regular Channel Settings" BaseMode="ADC_Regular_ConversionMode" Group="ADC_Regular_ConversionMode">
        <ConfigForMode>ADC_RegularChannelConfig</ConfigForMode>
        <Parameter Name="Rank"/>
        <Parameter Name="Channel"/>
        <Parameter Name="SamplingTime"/>
    </RefMode>
    <RefMode Name="cpChannelRegularConversion" Comment="Regular Channel Settings">
        <ConfigForMode>ADC_RegularChannelConfig</ConfigForMode>
        <Parameter Name="Rank"/>
        <Parameter Name="Channel"/>
        <Parameter Name="SamplingTime"/>
    </RefMode>
    <!-- Possible Value of Rank parameters -->
    <RefParameter Comment="Rank" DefaultValue="1" Max="16" Min="1" Name="Rank" Type="integer" Group="ADCChannel">
        <Description>The rank in the regular group sequencer</Description>
    </RefParameter>
	    
    <!-- Possible Value of Channel parameter -->
    <RefParameter Comment="Channel" DefaultValue="ADC_CHANNEL_0" Name="Channel" Type="list" Visible="true" Group="ADCChannel">
        <PossibleValue Comment="Channel 0" Value="ADC_CHANNEL_0"/>
        <PossibleValue Comment="Channel 1" Value="ADC_CHANNEL_1"/>
        <PossibleValue Comment="Channel 2" Value="ADC_CHANNEL_2"/>
        <PossibleValue Comment="Channel 3" Value="ADC_CHANNEL_3"/>
        <PossibleValue Comment="Channel 4" Value="ADC_CHANNEL_4"/>
        <PossibleValue Comment="Channel 5" Value="ADC_CHANNEL_5"/>
        <PossibleValue Comment="Channel 6" Value="ADC_CHANNEL_6"/>
        <PossibleValue Comment="Channel 7" Value="ADC_CHANNEL_7"/>
        <PossibleValue Comment="Channel 8" Value="ADC_CHANNEL_8"/>
        <PossibleValue Comment="Channel 9" Value="ADC_CHANNEL_9"/>
        <PossibleValue Comment="Channel 10" Value="ADC_CHANNEL_10"/>
        <PossibleValue Comment="Channel 11" Value="ADC_CHANNEL_11"/>
        <PossibleValue Comment="Channel 12" Value="ADC_CHANNEL_12"/>
        <PossibleValue Comment="Channel 13" Value="ADC_CHANNEL_13"/>
        <PossibleValue Comment="Channel 14" Value="ADC_CHANNEL_14"/>
        <PossibleValue Comment="Channel 15" Value="ADC_CHANNEL_15"/>
        <PossibleValue Comment="Channel Temperature Sensor" Value="ADC_CHANNEL_TEMPSENSOR"/>
        <PossibleValue Comment="Channel Vrefint" Value="ADC_CHANNEL_VREFINT"/>
        <PossibleValue Comment="Channel Vbat" Value="ADC_CHANNEL_VBAT"/>
    </RefParameter>
	    
    <!-- Possible Value of SamplingTime parameters  -->
    <RefParameter Comment="Sampling Time" DefaultValue="ADC_SAMPLETIME_1CYCLE_5" Name="SamplingTime" Type="list" Group="ADCChannel">
        <PossibleValue Comment="1.5 Cycles" Value="ADC_SAMPLETIME_1CYCLE_5"/>
        <PossibleValue Comment="7.5 Cycles" Value="ADC_SAMPLETIME_7CYCLES_5"/>
        <PossibleValue Comment="13.5 Cycles" Value="ADC_SAMPLETIME_13CYCLES_5"/>
        <PossibleValue Comment="28.5 Cycles" Value="ADC_SAMPLETIME_28CYCLES_5"/>
        <PossibleValue Comment="41.5 Cycles" Value="ADC_SAMPLETIME_41CYCLES_5"/>
        <PossibleValue Comment="55.5 Cycles" Value="ADC_SAMPLETIME_55CYCLES_5"/>
        <PossibleValue Comment="71.5 Cycles" Value="ADC_SAMPLETIME_71CYCLES_5"/>
        <PossibleValue Comment="239.5 Cycles" Value="ADC_SAMPLETIME_239CYCLES_5"/>
        <Description>Regular Channel x sampling time selection</Description>
    </RefParameter>

    <!-- Parameters of ChannelInjectedConversion Mode -->
    <RefMode Name="ChannelInjectedConversion" Comment="Injected Channel Settings" BaseMode="ADC_Injected_ConversionMode" Group="ADC_Injected_ConversionMode">
        <ConfigForMode>ADC_InjectedChannelConfig</ConfigForMode>
        <Parameter Name="Rank"/>
        <Parameter Name="Channel"/>
        <Parameter Name="SamplingTime"/>
        <Parameter Name="InjectedOffset"/>
    </RefMode>
     <RefMode Name="cpChannelInjectedConversion" Comment="Injected Channel Settings" >
        <ConfigForMode>ADC_InjectedChannelConfig</ConfigForMode>
        <Parameter Name="Rank"/>
        <Parameter Name="Channel"/>
        <Parameter Name="SamplingTime"/>
        <Parameter Name="InjectedOffset"/>
    </RefMode>
    <!-- Possible Value of InjectedOffset parameter -->
    <RefParameter Comment="Injected Offset" DefaultValue="0" Max="4095" Min="0" Name="InjectedOffset" Type="integer">
        <Description>Data offset for injected channel </Description>
    </RefParameter>
    
    <!-- WatchDog parameters -->
    <RefMode Name="WatchDog" Comment="" Group="WatchDog">
        <ConfigForMode>ADC_AnalogWDGConfig</ConfigForMode>
        <Parameter Name="EnableAnalogWatchDog"/>
        <Parameter Name="WatchdogMode"/>
        <Parameter Name="WatchdogChannel"/>
        <Parameter Name="AWD1HighThreshold"/>
        <Parameter Name="AWD1LowThreshold"/>
        <Parameter Name="AWD1ITMode"/>
    </RefMode>
        
    <!-- Possible value of EnableAnalogWatchDog parameter -->
    <RefParameter Comment="Enable Analog WatchDog Mode" DefaultValue="false" Name="EnableAnalogWatchDog" Type="boolean" Group="ADCgroup" Visible="false">
        <Condition Expression="(EnableRegularConversion=DISABLE)&amp;(InjNumberOfConversion=0)" Diagnostic=""/>
        <PossibleValue Comment="false" Value="false"/>
        <Description>Enable/Disable Analog WatchDog Mode</Description> 
    </RefParameter>
    <RefParameter Comment="Enable Analog WatchDog Mode" DefaultValue="false" Name="EnableAnalogWatchDog" Type="boolean" Group="ADCgroup">
        <PossibleValue Comment="true" Value="true"/>
        <PossibleValue Comment="false" Value="false"/>
        <Description>Enable/Disable Analog WatchDog Mode</Description> 
    </RefParameter>
	    
    <!-- Possible value of WatchdogMode parameters -->
    <RefParameter Comment="Watchdog Mode" DefaultValue="null" Name="WatchdogMode" Type="list" Group="ADCgroup" Visible="false">
        <Condition Expression="EnableAnalogWatchDog=false" Diagnostic=""/>
    </RefParameter>
    <RefParameter Comment="Watchdog Mode" DefaultValue="ADC_ANALOGWATCHDOG_SINGLE_REG" Name="WatchdogMode" Type="list" Group="ADCgroup">
        <Condition Expression="(InjNumberOfConversion=0) &amp; (EnableRegularConversion=ENABLE)" Diagnostic=""/>
        <PossibleValue Comment="Single regular channel" Value="ADC_ANALOGWATCHDOG_SINGLE_REG"  Semaphore="SingleRegular$IpInstance"/>
        <PossibleValue Comment="Single regular or injected channel" Value="ADC_ANALOGWATCHDOG_SINGLE_REGINJEC" Semaphore="SingleInjectedOrRegular$IpInstance"/>
        <PossibleValue Comment="All regular channels" Value="ADC_ANALOGWATCHDOG_ALL_REG" Semaphore="AllRegular$IpInstance"/>
        <PossibleValue Comment="All regular and injected channels" Value="ADC_ANALOGWATCHDOG_ALL_REGINJEC" Semaphore="AllRegInjected$IpInstance"/>
        <Description>Configures the ADC analog watchdog mode.</Description>
    </RefParameter>
	
	<RefParameter Comment="Watchdog Mode" DefaultValue="ADC_ANALOGWATCHDOG_SINGLE_INJEC" Name="WatchdogMode" Type="list" Group="ADCgroup">
        <Condition Expression="(!(InjNumberOfConversion=0)) &amp; (EnableRegularConversion=DISABLE)" Diagnostic=""/>
        <PossibleValue Comment="Single injected channel" Value="ADC_ANALOGWATCHDOG_SINGLE_INJEC" Semaphore="SingleInjected$IpInstance" />
		<PossibleValue Comment="Single regular or injected channel" Value="ADC_ANALOGWATCHDOG_SINGLE_REGINJEC" Semaphore="SingleInjectedOrRegular$IpInstance" />
		<PossibleValue Comment="All injected channels" Value="ADC_ANALOGWATCHDOG_ALL_INJEC" Semaphore="AllInjected$IpInstance"/>
        <PossibleValue Comment="All regular and injected channels" Value="ADC_ANALOGWATCHDOG_ALL_REGINJEC" Semaphore="AllRegInjected$IpInstance"/>

        <Description>Configures the ADC analog watchdog mode.</Description>
    </RefParameter>
	
    <RefParameter Comment="Watchdog Mode" DefaultValue="ADC_ANALOGWATCHDOG_SINGLE_REG" Name="WatchdogMode" Type="list" Group="ADCgroup">
        <PossibleValue Comment="Single regular channel" Value="ADC_ANALOGWATCHDOG_SINGLE_REG" Semaphore="SingleRegular$IpInstance" />
        <PossibleValue Comment="Single injected channel" Value="ADC_ANALOGWATCHDOG_SINGLE_INJEC" Semaphore="SingleInjected$IpInstance" />
        <PossibleValue Comment="Single regular or injected channel" Value="ADC_ANALOGWATCHDOG_SINGLE_REGINJEC" Semaphore="SingleInjectedOrRegular$IpInstance" />
        <PossibleValue Comment="All regular channels" Value="ADC_ANALOGWATCHDOG_ALL_REG" Semaphore="AllRegular$IpInstance"/>
        <PossibleValue Comment="All injected channels" Value="ADC_ANALOGWATCHDOG_ALL_INJEC" Semaphore="AllInjected$IpInstance"/>
        <PossibleValue Comment="All regular and injected channels" Value="ADC_ANALOGWATCHDOG_ALL_REGINJEC" Semaphore="AllRegInjected$IpInstance"/>
        <Description>Configures the ADC analog watchdog mode.</Description>
    </RefParameter>
        
    <!-- Possible value of WatchdogChannel parameter -->
    <RefParameter Comment="Analog WatchDog Channel" DefaultValue="null" Name="WatchdogChannel" Type="list" Group="ADCChannel" Visible="false">
        <Condition Expression="EnableAnalogWatchDog=false" Diagnostic=""/>
    </RefParameter>
    <RefParameter Comment="Analog WatchDog Channel" DefaultValue="null" Name="WatchdogChannel" Type="list" Group="ADCChannel" Visible="false">
        <Condition Expression="AllRegular$IpInstance" Diagnostic=""/>
    </RefParameter>
    <RefParameter Comment="Analog WatchDog Channel" DefaultValue="null" Name="WatchdogChannel" Type="list" Group="ADCChannel" Visible="false">
        <Condition Expression="AllInjected$IpInstance" Diagnostic=""/>
    </RefParameter>
    <RefParameter Comment="Analog WatchDog Channel" DefaultValue="null" Name="WatchdogChannel" Type="list" Group="ADCChannel" Visible="false">
        <Condition Expression="AllRegInjected$IpInstance|NoneWDG$IpInstance" Diagnostic=""/>
    </RefParameter>
    <!-- <RefParameter Comment="Analog WatchDog Channel" DefaultValue="null" Name="WatchdogChannel" Type="list" Group="ADCChannel" Visible="false">
            <Condition Expression="WatchdogMode=ADC_ANALOGWATCHDOG_ALL_REG" Diagnostic=""/>
    </RefParameter>
    <RefParameter Comment="Analog WatchDog Channel" DefaultValue="null" Name="WatchdogChannel" Type="list" Group="ADCChannel" Visible="false">
            <Condition Expression="WatchdogMode=ADC_ANALOGWATCHDOG_ALL_INJEC" Diagnostic=""/>
    </RefParameter>
    <RefParameter Comment="Analog WatchDog Channel" DefaultValue="null" Name="WatchdogChannel" Type="list" Group="ADCChannel" Visible="false">
            <Condition Expression="(WatchdogMode=ADC_ANALOGWATCHDOG_ALL_REGINJEC)|(WatchdogMode=ADC_ANALOGWATCHDOG_NONE)" Diagnostic=""/>
    </RefParameter> -->
    <RefParameter Comment="Analog WatchDog Channel" DefaultValue="" Name="WatchdogChannel" Type="list" Group="ADCChannel">
        <PossibleValue Comment="Channel 0" Value="ADC_CHANNEL_0"/>
        <PossibleValue Comment="Channel 1" Value="ADC_CHANNEL_1"/>
        <PossibleValue Comment="Channel 2" Value="ADC_CHANNEL_2"/>
        <PossibleValue Comment="Channel 3" Value="ADC_CHANNEL_3"/>
        <PossibleValue Comment="Channel 4" Value="ADC_CHANNEL_4"/>
        <PossibleValue Comment="Channel 5" Value="ADC_CHANNEL_5"/>
        <PossibleValue Comment="Channel 6" Value="ADC_CHANNEL_6"/>
        <PossibleValue Comment="Channel 7" Value="ADC_CHANNEL_7"/>
        <PossibleValue Comment="Channel 8" Value="ADC_CHANNEL_8"/>
        <PossibleValue Comment="Channel 9" Value="ADC_CHANNEL_9"/>
        <PossibleValue Comment="Channel 10" Value="ADC_CHANNEL_10"/>
        <PossibleValue Comment="Channel 11" Value="ADC_CHANNEL_11"/>
        <PossibleValue Comment="Channel 12" Value="ADC_CHANNEL_12"/>
        <PossibleValue Comment="Channel 13" Value="ADC_CHANNEL_13"/>
        <PossibleValue Comment="Channel 14" Value="ADC_CHANNEL_14"/>
        <PossibleValue Comment="Channel 15" Value="ADC_CHANNEL_15"/>
        <PossibleValue Comment="Channel 16" Value="ADC_CHANNEL_16"/>
        <PossibleValue Comment="Channel 17" Value="ADC_CHANNEL_17"/>
