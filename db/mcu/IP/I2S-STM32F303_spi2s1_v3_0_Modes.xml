<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<IP DBVersion="V3.0" IPType="peripheral" Name="I2S" Version="STM32F303_spi2s1_v3_0" xmlns:ns0="http://www.w3.org/2001/XMLSchema-instance" ns0:schemaLocation="http://mcd.rou.st.com/modules.php?name=mcu ../../db/IP_Modes_V3_0.xsd" xmlns="http://mcd.rou.st.com/modules.php?name=mcu">
	<ModeLogicOperator Name="OR">
		<Mode Name="Activate-External-Clock">
            <Semaphore>$IpInstance</Semaphore>
            <Condition Expression="!SPI$IpNumber" Diagnostic="SPI$IpNumber must be disabled"/>
			<ModeLogicOperator Name="OR">
				<Mode Name="Activate-External-Clock">
					<SignalLogicalOp Name="AND">
						<Signal Name="CKIN"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<Mode Name="Mode">
            <Semaphore>$IpInstance</Semaphore>
            <Condition Expression="!SPI$IpNumber" Diagnostic="SPI$IpNumber must be disabled"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="Master">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Master-With-Clock-Output">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="MCK"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Slave">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Full-Duplex-Master">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="EXT_SD"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Full-Duplex-Master-With-Clock-Output">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="EXT_SD"/>
						<Signal Name="MCK"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Full-Duplex-Slave">
					<SignalLogicalOp Name="AND">
						<Signal Name="CK"/>
						<Signal Name="EXT_SD"/>
						<Signal Name="SD"/>
						<Signal Name="WS"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
	</ModeLogicOperator>
	<RefSignal IOMode="GPIO" Name="CK"/>
	<RefSignal IOMode="GPIO" Name="CKIN"/>
	<RefSignal IOMode="GPIO" Name="EXT_SD"/>
	<RefSignal IOMode="GPIO" Name="MCK"/>
	<RefSignal IOMode="GPIO" Name="SD"/>
	<RefSignal IOMode="GPIO" Name="WS"/>
</IP>
