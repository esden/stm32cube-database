        <PossibleValue Value="FEC_15_4_G_NONE"             Comment="FEC_15_4_G_NONE" />
        <PossibleValue Value="FEC_15_4_G_NRNSC"            Comment="FEC_15_4_G_NRNSC"/>
        <PossibleValue Value="FEC_15_4_G_RSC"              Comment="FEC_15_4_G_RSC"/>
