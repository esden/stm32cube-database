        <PossibleValue Value="MOD_2FSK"               Comment="MOD_2FSK" />
        <PossibleValue Value="MOD_4FSK"               Comment="MOD_4FSK"/>
        <PossibleValue Value="MOD_2GFSK05"            Comment="MOD_2GFSK05"/>
