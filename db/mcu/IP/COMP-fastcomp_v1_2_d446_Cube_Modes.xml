		<PossibleValue Comment="Timer 1 Break Input 2 (BKIN2)" Value="COMP_OUTPUT_TIM1BKIN2"/>
		<PossibleValue Comment="Timer 1 Output Compare Reference Clear" Value="COMP_OUTPUT_TIM1OCREFCLR"/>
		<PossibleValue Comment="Timer 1 Input Capture 2" Value="COMP_OUTPUT_TIM1IC2"/>
		<PossibleValue Comment="Timer 2 Input Capture 3" Value="COMP_OUTPUT_TIM2IC3"/>
		<PossibleValue Action="Remove" Comment="Timer 8 Break Input (BKIN)" Condition="!TIM8_Exist" Value="COMP_OUTPUT_TIM8BKIN"/>
		<PossibleValue Action="Remove" Comment="Timer 8 Break Input 2 (BKIN2)" Condition="!TIM8_Exist" Value="COMP_OUTPUT_TIM8BKIN2"/>
		<PossibleValue Action="Remove" Comment="Timer 8 Output Compare Reference Clear" Condition="!TIM8_Exist" Value="COMP_OUTPUT_TIM8OCREFCLR"/>
		<PossibleValue Action="Remove" Comment="Timer 8 BKIN2 or Timer 1 BKIN2" Condition="!TIM8_Exist" Value="COMP_OUTPUT_TIM1BKIN2_TIM8BKIN2"/>
