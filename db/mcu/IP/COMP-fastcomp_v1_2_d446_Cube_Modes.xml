		<PossibleValue Action="Remove" Comment="Timer 20 Break Input 2 (BKIN2)" Condition="!TIM20_Exist" Value="COMP_OUTPUT_TIM20BKIN2"/>
		<PossibleValue Action="Remove" Comment="Timer 20 BKIN2, Timer 8 BKIN2 or Timer 1 BKIN2" Condition="!(TIM8_Exist &amp; TIM20_Exist)" Value="COMP_OUTPUT_TIM1BKIN2_TIM8BKIN2_TIM20BKIN2"/>
	</RefParameter>
	<RefParameter Comment="Output Internal Selection" DefaultValue="COMP_OUTPUT_NONE" Group="Output Parameters" Name="Output" Type="list" Visible="false">
		<PossibleValue Comment="None" Value="COMP_OUTPUT_NONE"/>
