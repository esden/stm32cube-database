					<Semaphore>WakeUp_Outputed</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_WAKEUP_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Timestamp">
			<ModeLogicOperator Name="XOR">
				<Mode Name="Timestamp enabled - Input Enabled to TS" UserName="Timestamp Routed to TS">
					<SignalLogicalOp Name="AND">
						<Signal Name="TS"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_TIMESTAMP_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Tamper 1">
			<ModeLogicOperator Name="XOR">
				<Mode Name="Tamper 1 enabled - Input Enabled to TAMP1" UserName="Tamper1 Routed to TAMP1">
					<SignalLogicalOp Name="AND">
						<Signal Name="TAMP1"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>Tamper1_ON</Semaphore>
			<Semaphore>RTC_TAMPER_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Tamper 2">
			<ModeLogicOperator Name="XOR">
				<Mode Name="Tamper 2 enabled - Input Enabled to TAMP2" UserName="Tamper2 Routed to TAMP2">
					<SignalLogicalOp Name="AND">
						<Signal Name="TAMP2"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>Tamper2_ON</Semaphore>
			<Semaphore>RTC_TAMPER_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Tamper 3">
			<ModeLogicOperator Name="XOR">
				<Mode Name="Tamper 3 enabled - Input Enabled to TAMP3" UserName="Tamper3 Routed to TAMP3">
