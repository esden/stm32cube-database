		</Parameter>	
	</RefMode>
	<RefMode Name="VREFINT_34">
		<Parameter Name="InvertingInput">
			<PossibleValue>COMP_INVERTINGINPUT_3_4VREFINT</PossibleValue>
		</Parameter>	
	</RefMode>
	<RefMode Name="VREFINT_12">
		<Parameter Name="InvertingInput">
			<PossibleValue>COMP_INVERTINGINPUT_1_2VREFINT</PossibleValue>
		</Parameter>	
	</RefMode>
	<RefMode Name="VREFINT_14">
		<Parameter Name="InvertingInput">
			<PossibleValue>COMP_INVERTINGINPUT_1_4VREFINT</PossibleValue>
		</Parameter>	
	</RefMode>
	<RefMode Name="INM_DACOUT1">
		<Parameter Name="InvertingInput">
			<PossibleValue>COMP_INVERTINGINPUT_DAC1</PossibleValue>
		</Parameter>	
	</RefMode>
	<RefMode Name="INM_DACOUT2">
		<Parameter Name="InvertingInput">
			<PossibleValue>COMP_INVERTINGINPUT_DAC2</PossibleValue>
		</Parameter>	
	</RefMode>
		
	
	<!-- Modes for COMP. Defined for BigScorpio & Scorpio2 (Category 2/3/4/5). Caution : COMP1 do not work with an ADC activated unless in Windows mode ( Bug 315353)-->
	<ModeLogicOperator Name="OR">
		<Mode Name="InputPlus" UserName="Input [+]">
			<ModeLogicOperator Name="XOR">
				<Mode Name="INP" ShrinkName="Input [+] : INP" UserName="INP">
					<SignalLogicalOp Name="AND">
						<Signal Name="INP"/>
					</SignalLogicalOp>
					<Semaphore>$IpInstance_ExternalInputPlus</Semaphore>
		                	<Condition Diagnostic="ADC must be disabled" Expression="(($IpNumber=1) &amp; !USE_ADC) | ($IpNumber=2)"/>
				</Mode>
				<Mode Name="INP_OPAMP1" RemoveCondition="!(OPAMP1:$modeexist_Filter) | ($IpNumber=2)" UserName="OPAMP 1">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_OPAMP1"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'OPAMP 1' activated" Expression="OPAMP1 &amp;(($IpNumber=1) &amp; !USE_ADC) | ($IpNumber=2) "/>
				</Mode>
				<Mode Name="INP_OPAMP2" RemoveCondition="!(OPAMP2:$modeexist_Filter) | ($IpNumber=2)" UserName="OPAMP 2">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_OPAMP2"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'OPAMP 2' activated" Expression="OPAMP2 &amp;(($IpNumber=1) &amp; !USE_ADC) | ($IpNumber=2) "/>
				</Mode>
				<Mode Name="INP_OPAMP3" RemoveCondition="!(OPAMP3:$modeexist_Filter) | ($IpNumber=2)" UserName="OPAMP 3">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_OPAMP3"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'OPAMP 3' activated" Expression="OPAMP3 &amp;(($IpNumber=1) &amp; !USE_ADC) | ($IpNumber=2) "/>
				</Mode>
				<Mode Name="WindowMode" RemoveCondition="($IpNumber=2)" UserName="Window Mode">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_WindowMode"/>
					</SignalLogicalOp>
					<Semaphore>$IpInstance_WindowsMode</Semaphore>
					<Condition Diagnostic="Need to have an INP (External Input [+]) on next Comparator" Expression="(($IpNumber=1) &amp; COMP2_ExternalInputPlus)"/>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>$IpInstance_InputPlus</Semaphore>
		</Mode>
		
		<Mode Name="InputNeg" RemoveCondition="($IpNumber=1)" UserName="Input [-]">
			<ModeLogicOperator Name="XOR">
				<Mode Name="INM" UserName="INM">
					<SignalLogicalOp Name="AND">
						<Signal Name="INM"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="INM_DACOUT1" RemoveCondition="!(DAC:$modeexist_DAC_OUT1)" UserName="DAC OUT1">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DACOUT1"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out 1' activated" Expression="(DAC_OUT1_Conf)"/>
				</Mode>
				<Mode Name="INM_DACOUT2" RemoveCondition="!(DAC:$modeexist_DAC_OUT2)" UserName="DAC OUT2">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DACOUT2"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out 2' activated" Expression="(DAC_OUT2_Conf)"/>
				</Mode>
				<Mode Name="VREFINT" UserName="Internal VRef">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_VREFINT"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="VREFINT_34" UserName="3/4 Internal VRef">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_VREFINT34"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="VREFINT_12" UserName="1/2 Internal VRef">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_VREFINT12"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="VREFINT_14" UserName="1/4 Internal VRef">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_VREFINT14"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Condition Diagnostic="Need to have an INP (External Input [+]) activated" Expression="$IpInstance_InputPlus"/>
		</Mode>
	</ModeLogicOperator>
	
	<!-- Signals for COMP. -->
	<RefSignal IOMode="Analog" Name="INP"/>
	<RefSignal IOMode="Analog" Name="INM"/>
	<RefSignal IOMode="AlternateFunctionPushPull" Name="OUT"/>

	<RefSignal Name="VS_WindowMode" Virtual="true"/>
	<RefSignal Name="VS_VREFINT" Virtual="true"/>
	<RefSignal Name="VS_VREFINT34" Virtual="true"/>
	<RefSignal Name="VS_VREFINT12" Virtual="true"/>
	<RefSignal Name="VS_VREFINT14" Virtual="true"/>
	<RefSignal Name="VS_DACOUT1" Virtual="true"/>
	<RefSignal Name="VS_DACOUT2" Virtual="true"/>
	<RefSignal Name="VS_OPAMP1" Virtual="true"/>
	<RefSignal Name="VS_OPAMP2" Virtual="true"/>
	<RefSignal Name="VS_OPAMP3" Virtual="true"/>
	
	<!-- Semaphore for COMP -->
	<Semaphore>$IpInstance</Semaphore>
</IP>