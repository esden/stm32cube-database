	<ModeLogicOperator Name="OR">
		<Mode Name="Alarm A">
                        <Condition Expression="!SYS_$IpInstance" Diagnostic="RTC is already used for system time base"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="Alarm A" UserName="Internal Alarm">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_RTC_Alarm_A_Intern"/>
					</SignalLogicalOp>
					<Semaphore>AlarmA_Intern</Semaphore>
				</Mode>
				<Mode Name="Alarm A" UserName="Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
					<Semaphore>AlarmA_Outputed</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_ALARM_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Alarm B">
                        <Condition Expression="!SYS_$IpInstance" Diagnostic="RTC is already used for system time base"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Alarm B" UserName="Internal Alarm">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_RTC_Alarm_B_Intern"/>
					</SignalLogicalOp>
					<Semaphore>AlarmB_Intern</Semaphore>
				</Mode>
				<Mode Name="Alarm B" UserName="Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
					<Semaphore>AlarmB_Outputed</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_ALARM_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="WakeUp">
                        <Condition Expression="!SYS_$IpInstance" Diagnostic="RTC is already used for system time base"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="WakeUp" UserName="Internal WakeUp">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_RTC_WakeUp_intern"/>
					</SignalLogicalOp>
					<Semaphore>WakeUp_Intern</Semaphore>
				</Mode>
				<Mode Name="WakeUp" UserName="Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
					<Semaphore>WakeUp_Outputed</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_WAKEUP_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Timestamp">
                <Condition Expression="!SYS_$IpInstance" Diagnostic="RTC is already used for system time base"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="Timestamp enabled - Input Enabled to AF1" UserName="Timestamp Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Timestamp enabled - Input Enabled to AF2" UserName="Timestamp Routed to AF2">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF2"/>
					</SignalLogicalOp>
					<Semaphore>TIMSTAMP_ON_AF2_ENABLED</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_TIMESTAMP_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Tamper 1">
                        <Condition Expression="!SYS_$IpInstance" Diagnostic="RTC is already used for system time base"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Tamper 1 enabled - Input Enabled to AF1" UserName="Tamper Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Tamper 1 enabled - Input Enabled to AF2" UserName="Tamper Routed to AF2">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF2"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_TAMPER_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Calibration 512Hz">
                        <Condition Expression="!SYS_$IpInstance" Diagnostic="RTC is already used for system time base"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Calibration_512Hz" UserName="Calibration 512Hz">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>		
		<Mode Name="Reference clock detection">
                        <Condition Expression="!SYS_$IpInstance" Diagnostic="RTC is already used for system time base"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Reference_Clock_Detection_Activate" UserName="Reference Clock detection">
					<SignalLogicalOp Name="AND">
						<Signal Name="REFIN"/>
					</SignalLogicalOp>
					<Semaphore>Prediv_With_REFIN</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
	</ModeLogicOperator>
