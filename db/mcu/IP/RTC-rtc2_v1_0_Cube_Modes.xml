	<!-- #################################################################################################-->
	<!--############################################# Modes Definition ######################################-->
	<!--#################################################################################################-->
	
	<ModeLogicOperator Name="OR">
	
	<Mode Name="Activate Clock Source">
			<ModeLogicOperator Name="XOR">
				<Mode Name="RTC_Enabled" UserName="Activate Clock Source">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_RTC_Activate"/>
					</SignalLogicalOp>
					<Semaphore>RTC_ENABLED</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		
		<Mode Name="Activate Calendar">
			<Condition Diagnostic="RTC is already used for system time base" Expression="!SYS_$IpInstance &amp; RTC_ENABLED"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="RTC_Calendar" UserName="Activate Calendar">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_RTC_Calendar"/>
					</SignalLogicalOp>
					<Semaphore>RTC_CALENDAR</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		<Mode Name="Alarm A">
            <Condition Diagnostic="RTC is already used for system time base" Expression="!SYS_$IpInstance &amp; RTC_CALENDAR"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="Alarm A" UserName="Internal Alarm">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_RTC_Alarm_A_Intern"/>
					</SignalLogicalOp>
					<Semaphore>AlarmA_Intern</Semaphore>
				</Mode>
				<Mode Name="AlarmA_RoutedAF" UserName="Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
					<Semaphore>AlarmA_Outputed</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_ALARM_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Alarm B">
                        <Condition Diagnostic="RTC is already used for system time base" Expression="!SYS_$IpInstance &amp; RTC_CALENDAR"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Alarm B" UserName="Internal Alarm">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_RTC_Alarm_B_Intern"/>
					</SignalLogicalOp>
					<Semaphore>AlarmB_Intern</Semaphore>
				</Mode>
				<Mode Name="AlarmB_RoutedAF" UserName="Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
					<Semaphore>AlarmB_Outputed</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_ALARM_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="WakeUp">
                        <Condition Diagnostic="RTC is already used for system time base" Expression="!SYS_$IpInstance &amp; RTC_CALENDAR"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="WakeUp" UserName="Internal WakeUp">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_RTC_WakeUp_intern"/>
					</SignalLogicalOp>
					<Semaphore>WakeUp_Intern</Semaphore>
				</Mode>
				<Mode Name="WakeUpAF1" UserName="Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
					<Semaphore>WakeUp_Outputed</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_WAKEUP_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Timestamp">
                <Condition Diagnostic="RTC is already used for system time base" Expression="!SYS_$IpInstance &amp; RTC_CALENDAR"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="Timestamp enabled - Input Enabled to AF1" UserName="Timestamp Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Timestamp enabled - Input Enabled to AF2" UserName="Timestamp Routed to AF2">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF2"/>
					</SignalLogicalOp>
					<Semaphore>TIMSTAMP_ON_AF2_ENABLED</Semaphore>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_TIMESTAMP_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Tamper 1">
                        <Condition Diagnostic="RTC is already used for system time base" Expression="!SYS_$IpInstance &amp; RTC_ENABLED"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Tamper 1 enabled - Input Enabled to AF1" UserName="Tamper Routed to AF1">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="Tamper 1 enabled - Input Enabled to AF2" UserName="Tamper Routed to AF2">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF2"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>RTC_TAMPER_INTERRUPT</Semaphore>
		</Mode>
		<Mode Name="Calibration 512Hz">
                        <Condition Diagnostic="RTC is already used for system time base" Expression="!SYS_$IpInstance &amp; RTC_ENABLED"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Calibration_512Hz" UserName="Calibration 512Hz">
					<SignalLogicalOp Name="AND">
						<Signal Name="AF1"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>		
		<Mode Name="Reference clock detection">
                        <Condition Diagnostic="RTC is already used for system time base" Expression="!SYS_$IpInstance &amp; RTC_ENABLED"/>                    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Reference_Clock_Detection_Activate" UserName="Reference Clock detection">
					<SignalLogicalOp Name="AND">
						<Signal Name="REFIN"/>
					</SignalLogicalOp>
					<Semaphore>Prediv_With_REFIN</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
	</ModeLogicOperator>
