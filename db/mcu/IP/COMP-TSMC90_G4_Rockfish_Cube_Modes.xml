	<!-- Modes for COMP. Defined for MantaEdge (F302/303/358) -->
	<ModeLogicOperator Name="OR">
		<Mode Name="NonInvertingInput" UserName="Input [+]">
			<ModeLogicOperator Name="XOR">
				<Mode Name="INP" ShrinkName="Input [+] : INP" UserName="INP">
					<SignalLogicalOp Name="AND">
						<Signal Name="INP"/>
					</SignalLogicalOp>
					<Semaphore>$IpInstance_ExternalInputPlus</Semaphore>
	                                <Semaphore>Semaphore_$IpInstance_Enable</Semaphore>
				</Mode>
		
			</ModeLogicOperator>
			<Semaphore>$IpInstance_InputPlus</Semaphore>

		</Mode>
		
		<Mode Name="InputNeg" UserName="Input [-]">
			<ModeLogicOperator Name="XOR">
				<Mode Name="INM" UserName="INM">
					<SignalLogicalOp Name="AND">
						<Signal Name="INM"/>
					</SignalLogicalOp>
				</Mode>

				<Mode Name="INM_DAC1OUT1" UserName="DAC1 OUT1" RemoveCondition="!(($IpNumber=1)|($IpNumber=3)|($IpNumber=4))">
                                       <Semaphore>$IpInstance_DAC1Channel1</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DAC1OUT1"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out' activated with internal connection activated" Expression="(DAC1_OUT1_ExtAndInt | DAC1_OUT1_Int)"/>
				</Mode>

                        	<Mode Name="INM_DAC1OUT2" UserName="DAC1 OUT2" RemoveCondition="!(($IpNumber=2)|($IpNumber=5))">
                                       <Semaphore>$IpInstance_DAC1Channel2</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DAC1OUT2"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out' activated with internal connection activated" Expression="(DAC1_OUT2_ExtAndInt | DAC1_OUT2_Int)"/>

				</Mode>

                        	<Mode Name="INM_DAC2OUT1" UserName="DAC2 OUT1" RemoveCondition="!(($IpNumber=6)|($IpNumber=7))">
                                       <Semaphore>$IpInstance_DAC2Channel1</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DAC2OUT1"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out' activated with internal connection activated" Expression="(DAC2_OUT1_ExtAndInt | DAC2_OUT1_Int)"/>
				</Mode>

                        
                        	<Mode Name="INM_DAC3OUT1" UserName="DAC3 OUT1" RemoveCondition="!(($IpNumber=1)|($IpNumber=3))">
                                       <Semaphore>$IpInstance_DAC3Channel1</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DAC3OUT1"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out' activated with internal connection activated" Expression="(DAC3_OUT1_ExtAndInt | DAC3_OUT1_Int)"/>
				</Mode>


                        	<Mode Name="INM_DAC3OUT2" UserName="DAC3 OUT2" RemoveCondition="!(($IpNumber=2)|($IpNumber=4))">
                                       <Semaphore>$IpInstance_DAC3Channel2</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DAC3OUT2"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out' activated with internal connection activated" Expression="(DAC3_OUT2_ExtAndInt | DAC3_OUT2_Int)"/>
				</Mode>




                        	<Mode Name="INM_DAC4OUT1" UserName="DAC4 OUT1" RemoveCondition="!(($IpNumber=5)|($IpNumber=7))">
                                       <Semaphore>$IpInstance_DAC4Channel1</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DAC4OUT1"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out' activated with internal connection activated" Expression="(DAC4_OUT1_ExtAndInt | DAC4_OUT1_Int)"/>
				</Mode>


                        	<Mode Name="INM_DAC4OUT2" UserName="DAC4 OUT2" RemoveCondition="!($IpNumber=6)">
                                       <Semaphore>$IpInstance_DAC4Channel2</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_DAC4OUT2"/>
					</SignalLogicalOp>
					<Condition Diagnostic="Need to have an 'DAC Out' activated with internal connection activated" Expression="(DAC4_OUT2_ExtAndInt | DAC4_OUT2_Int)"/>
				</Mode>


		
				<Mode Name="VREFINT" UserName="Internal VRef">
                                       <Semaphore>$IpInstance_Vref</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_VREFINT"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="VREFINT_34" UserName="3/4 Internal VRef">
                                       <Semaphore>$IpInstance_Vref34</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_VREFINT34"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="VREFINT_12" UserName="1/2 Internal VRef">
                                       <Semaphore>$IpInstance_Vref12</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_VREFINT12"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="VREFINT_14" UserName="1/4 Internal VRef">
                                       <Semaphore>$IpInstance_Vref14</Semaphore>
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_VREFINT14"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Condition Diagnostic="Need to have an INP (External Input [+]) activated" Expression="$IpInstance_InputPlus"/>
		</Mode>
		
		<Mode Name="ExternalOutput">
                   <ModeLogicOperator Name="OR">
				<Mode Name="ExternalOutput" UserName="External Output">
					<SignalLogicalOp Name="AND">
                                     	    <Signal Name="OUT"/>
 					</SignalLogicalOp>
				</Mode>
                        </ModeLogicOperator>
			<Condition Diagnostic="Need to have an INP (External Input [+]) activated" Expression="$IpInstance_InputPlus"/>
		</Mode>
               
