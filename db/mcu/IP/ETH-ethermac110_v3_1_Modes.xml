				</Mode>	
				<Mode Name="MII_PTP_Synchro" RemoveCondition="!UFBGA144 &amp; !STM32H7R7A8Ix &amp; !STM32H7S7A8Ix &amp; !WLCSP &amp; !VFQFPN68 &amp; !LQFP100 &amp; !LQFP144">
