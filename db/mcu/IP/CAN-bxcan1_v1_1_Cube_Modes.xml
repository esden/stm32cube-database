        <Parameter Name="FilterMaskIdHigh"/>
        <Parameter Name="FilterMaskIdLow"/>
        <Parameter Name="FilterFIFOAssignment"/>
        -->
        <!-- Advanced/Debug Parameters -->
        <Parameter Name="Mode"/>
    </RefMode>
	
    <!-- RefMode for 'Slave' CAN Mode. -->
    <!-- For the Moment, only one Filter can be Parameter (90% of use cases). -->
    <RefMode Name="Slave" BaseMode="canBasic">
        <ConfigForMode>Can_Init</ConfigForMode>
                
      
       <!-- 'Bit Timing' Parameters -->
		<Parameter Name="CalculateTimeQuantum"/>
		<Parameter Name="BS1"/>
		<Parameter Name="BS2"/>
		
		<Parameter Name="SJW"/>
		<!-- 'Transmission' Parameters -->
		<Parameter Name="TTCM"/>
		<Parameter Name="ABOM"/>
		<Parameter Name="AWUM"/>
		<Parameter Name="NART"/>
		<Parameter Name="RFLM"/>
		<Parameter Name="TXFP"/>
        <!-- 'Filter' Parameters  -->
        <!--
