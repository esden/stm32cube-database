        <!-- Advanced/Debug Parameters -->
        <Parameter Name="Mode"/>
    </RefMode>	
	
	
    <!-- Modes for CAN. Suppose Silent and/or Loopback Modes are Debug, and need Tx/Rx on Board -->
    <ModeLogicOperator Name="OR">
        <Mode Name="Mode" UserName="Mode">
            <ModeLogicOperator Name="XOR">
                <Mode Name="Master" UserName="Master"  ShrinkName="Master Mode" RemoveCondition="$IpNumber=2" >
                    <SignalLogicalOp Name="AND">
                        <Signal Name="RX"/>
                        <Signal Name="TX"/>
                    </SignalLogicalOp>
                    <Condition Expression="$IpNumber=1" Diagnostic="Master active only on CAN1"/>
				
                </Mode>
				
                <Mode Name="Slave" UserName="Slave" ShrinkName="Slave Mode" RemoveCondition="$IpNumber=1" >
                    <SignalLogicalOp Name="AND">
                        <Signal Name="RX"/>
                        <Signal Name="TX"/>
                    </SignalLogicalOp>
                    <Condition Expression="$IpNumber=2" Diagnostic="Slave active only on CAN2"/>
                </Mode>
            </ModeLogicOperator>
            <Semaphore>USB_OR_CAN</Semaphore>
        </Mode>
    </ModeLogicOperator>
		
    <!-- RefSignal for CAN -->
    <RefSignal IOMode="AlternateFunctionPushPull" Name="RX"/>
    <RefSignal IOMode="AlternateFunctionPushPull" Name="TX"/>
</IP>
