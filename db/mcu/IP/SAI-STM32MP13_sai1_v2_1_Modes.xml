	<!-- ModeLogicOperator for SAI -->
	<ModeLogicOperator Name="XOR">
		<Mode Name="SAI_A" UserName="SAI A">
			<ModeLogicOperator Name="OR">			
				<Mode Name="SAI_A" UserName="Mode">
					<ModeLogicOperator Name="XOR">
						<Mode Name="SAI_A_Master" UserName="Master">							
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_A"/>
								<Signal Name="SCK_A"/>
								<Signal Name="FS_A"/>
							</SignalLogicalOp>
							<Semaphore>$IpInstance_SAIA_Master</Semaphore>	
							<Semaphore>SAIA_Master</Semaphore>	
							<Semaphore>SAI_Master</Semaphore>	
						</Mode>						
						<Mode Name="SAI_A_MasterWithClock" UserName="Master with Master Clock Out">
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_A"/>
								<Signal Name="SCK_A"/>
								<Signal Name="FS_A"/>
								<Signal Name="MCLK_A"/>
							</SignalLogicalOp>
							<Semaphore>$IpInstance_SAIA_Master</Semaphore>	
							<Semaphore>SAIA_Master</Semaphore>
							<Semaphore>SAI_Master</Semaphore>
						</Mode>
                                                <Mode Name="SAI_A_SlaveWithClock" UserName="Slave with Master Clock Out">
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_A"/>
								<Signal Name="SCK_A"/>
								<Signal Name="FS_A"/>
								<Signal Name="MCLK_A"/>
							</SignalLogicalOp>
							<Semaphore>$IpInstance_SAIA_Slave</Semaphore>	
							<Semaphore>SAIA_Slave</Semaphore>
							<Semaphore>SAI_Slave</Semaphore>
						</Mode>
						<Mode Name="SAI_A_AsyncSlave" UserName="Asynchronous Slave">
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_A"/>
								<Signal Name="SCK_A"/>
								<Signal Name="FS_A"/>
							</SignalLogicalOp>
							<Semaphore>$IpInstance_SAIA_AsyncSlave</Semaphore>
							<Semaphore>$IpInstance_SAIA_Slave</Semaphore>
							<Semaphore>SAIA_Slave</Semaphore>
							<Semaphore>SAI_AsyncSlave</Semaphore>
							<!--<Condition Expression="!$IpInstance_SAIA_SPDIF" Diagnostic=""/>-->
						</Mode>						
						<Mode Name="SAI_A_SyncSlave" RemoveCondition="((!$ModeExist_SAI_B_Master) &amp; (!$ModeExist_SAI_B_MasterWithClock) &amp; (!$ModeExist_SAI_B_AsyncSlave))" ShrinkName="SAI_A in Synchronous Slave Mode" UserName="Synchronous Slave"> 
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_A"/>
							</SignalLogicalOp>
							<Semaphore>$IpInstance_SAIA_SyncSlave</Semaphore>
							<Semaphore>$IpInstance_SAIA_Slave</Semaphore>
							<Semaphore>SAIA_Slave</Semaphore>
							<!--<Condition Expression="($IpInstance_SAIB_Master | $IpInstance_SAIB_AsyncSlave) &amp; (!$IpInstance_SAIA_SPDIF)" Diagnostic="Need to have SAI_B on 'Master' or 'Asynchronous Slave' Mode"/>-->
							<Condition Diagnostic="Need to have SAI_B on 'Master' or 'Asynchronous Slave' Mode" Expression="($IpInstance_SAIB_Master | $IpInstance_SAIB_AsyncSlave)"/>
							<!-- Ticket 60119 -->
							<!--Condition Diagnostic="Need to have one SAI on 'Master' or 'Asynchronous Slave' Mode" Expression="(SAI_Master | SAI_AsyncSlave)"/-->
						</Mode>							
						<Mode Name="SAI_A_SPDIF" UserName="SPDIF TX Transmitter (IEC60958)">
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_A"/>						
							</SignalLogicalOp>	
							<Semaphore>$IpInstance_SAIA_SPDIF</Semaphore>
						</Mode>	
						<!-- 69799 ThePDM must be activate if we have at least D1 and CK1 or CK2 avaialble -->						
						<Mode Name="PDM_Master_Mode_CK1" UserName="Pulse Density Modulation using MicroPhones pairs (1-2) and CK1 as clock"  >
							<SignalLogicalOp Name="AND">																
								<Signal Name="D1"/>	
								<Signal Name="CK1"/>								
							</SignalLogicalOp>
							<Semaphore>$IpInstance_PDM_CK1</Semaphore>
							<Semaphore>$IpInstance_PDM</Semaphore>
							<Semaphore>SEM_CK1</Semaphore>
							<Semaphore>SEM_MIC_1</Semaphore>
						</Mode>						
						<Mode Name="PDM_Master_Mode_CK2" UserName="Pulse Density Modulation using MicroPhones pairs (1-2) and CK2 as clock"  >
							<SignalLogicalOp Name="AND">																	
								<Signal Name="D1"/>	
								<Signal Name="CK2"/>
							</SignalLogicalOp>
							<Semaphore>$IpInstance_PDM</Semaphore>
							<Semaphore>$IpInstance_PDM_CK2</Semaphore>
							<Semaphore>SEM_CK2</Semaphore>
							<Semaphore>SEM_MIC_1</Semaphore>
						</Mode>						
						
					</ModeLogicOperator>
					<Semaphore>$IpInstance_SAIA_Mode</Semaphore>
					<Semaphore>SAIAUsed_ForRCC</Semaphore>
					<Semaphore>$IpInstance_SAIAUsed_ForRCC</Semaphore>					
					<Semaphore>$IpInstanceUsed_ForRCC</Semaphore>
					<Semaphore>$IpInstance_SAIAUsed_ForDMA</Semaphore>
				</Mode>				
					
				<Mode Name="NumberOfMicroPhones" UserName="Number Of MicroPhones">
					<ModeLogicOperator Name="XOR">												
						<Mode Name="3_4_MicroPhones" UserName="Add MicroPhones pairs (3-4)">
							<SignalLogicalOp Name="AND">																		
								<Signal Name="D2"/>															
							</SignalLogicalOp>	
							<Semaphore>SEM_MIC_2</Semaphore>
						</Mode>						
						<Mode Name="5_6_MicroPhones" UserName="Add MicroPhones paris (5-6)">
							<SignalLogicalOp Name="AND">																
								<Signal Name="D2"/>	
								<Signal Name="D3"/>															
							</SignalLogicalOp>
							<Semaphore>SEM_MIC_3</Semaphore>
						</Mode>
						<Mode Name="7_8_MicroPhones" UserName="Add MicroPhones paris (7-8)">
							<SignalLogicalOp Name="AND">																	
								<Signal Name="D2"/>	
								<Signal Name="D3"/>
								<Signal Name="D4"/>	
							</SignalLogicalOp>
							<Semaphore>SEM_MIC_4</Semaphore>
						</Mode>		
					</ModeLogicOperator>
					<Condition Expression="$IpInstance_PDM" Diagnostic="Pulse Density Modulation mode disabled"/>
				</Mode>
				<Mode Name="Clock_Selection" UserName="Add Clock">
					<ModeLogicOperator Name="XOR">							
						<Mode Name="CK1" UserName="CK1" RemoveCondition="$IpInstance_PDM_CK1">
							<SignalLogicalOp Name="AND">								
								<Signal Name="CK1"/>									
							</SignalLogicalOp>
							<Semaphore>SEM_CK1_CK2</Semaphore>							
						</Mode>							
						<Mode Name="CK2" UserName="CK2" RemoveCondition="$IpInstance_PDM_CK2">											
							<SignalLogicalOp Name="AND">								
								<Signal Name="CK2"/>									
							</SignalLogicalOp>
							<Semaphore>SEM_CK1_CK2</Semaphore>	
						</Mode>							
						
					</ModeLogicOperator>	
					<Condition Expression="$IpInstance_PDM" Diagnostic="Pulse Density Modulation mode disabled"/>							
				</Mode>
			</ModeLogicOperator>
		</Mode>
	
	
		
	<Mode Name="SAI_B" UserName="SAI B">
			<ModeLogicOperator Name="OR">			
				<Mode Name="SAI_B" UserName="Mode">
					<ModeLogicOperator Name="XOR">
						<Mode Name="SAI_B_Master" UserName="Master">							
