						<Mode Name="SAI_B_AsyncSlave" UserName="Asynchronous Slave">
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_B"/>
								<Signal Name="SCK_B"/>
								<Signal Name="FS_B"/>
							</SignalLogicalOp>
							<Semaphore>$IpInstance_SAIB_AsyncSlave</Semaphore>
							<Semaphore>SAIB_Slave</Semaphore>
							<Semaphore>SAI_AsyncSlave</Semaphore>
							<!--<Condition Expression="!$IpInstance_SAIB_SPDIF" Diagnostic=""/>-->
						</Mode>						
						<Mode Name="SAI_B_SyncSlave" RemoveCondition="((!$ModeExist_SAI_A_Master) &amp; (!$ModeExist_SAI_A_MasterWithClock) &amp; (!$ModeExist_SAI_A_AsyncSlave))" ShrinkName="SAI_B in Synchronous Slave Mode" UserName="Synchronous Slave"> 
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_B"/>
							</SignalLogicalOp>
							<Semaphore>$IpInstance_SAIB_SyncSlave</Semaphore>
							<Semaphore>SAIB_Slave</Semaphore>
							<!--<Condition Expression="($IpInstance_SAIA_Master | $IpInstance_SAIA_AsyncSlave) &amp; (!$IpInstance_SAIB_SPDIF)" Diagnostic="Need to have SAI_A on 'Master' or 'Asynchronous Slave' Mode"/>-->
							<Condition Diagnostic="Need to have SAI_A on 'Master' or 'Asynchronous Slave' Mode" Expression="($IpInstance_SAIA_Master | $IpInstance_SAIA_AsyncSlave)"/>
							<!-- Ticket 60119 -->
							<!--Condition Diagnostic="Need to have one SAI on 'Master' or 'Asynchronous Slave' Mode" Expression="(SAI_Master | SAI_AsyncSlave)"/-->
						</Mode>							
						<Mode Name="SAI_B_SPDIF" UserName="SPDIF TX Transmitter (IEC60958)">
							<SignalLogicalOp Name="AND">
								<Signal Name="SD_B"/>						
							</SignalLogicalOp>	
							<Semaphore>$IpInstance_SAIB_SPDIF</Semaphore>							
						</Mode>						
					</ModeLogicOperator>
					<Semaphore>$IpInstance_SAIB_Mode</Semaphore>
					<Semaphore>SAIBUsed_ForRCC</Semaphore>
					<Semaphore>$IpInstance_SAIBUsed_ForRCC</Semaphore>
					<Semaphore>$IpInstanceUsed_ForRCC</Semaphore>
					<Semaphore>$IpInstance_SAIBUsed_ForDMA</Semaphore>
				</Mode>							
												
			</ModeLogicOperator>
		</Mode>	
		<Mode Name="SynchroExternal" UserName="External Synchro Out">
			<ModeLogicOperator Name="OR">
				<Mode Name="SynchroExternal" UserName="External Synchro Out">
					<ModeLogicOperator Name="XOR">
						<Mode Name="SynchroExternal" UserName="External Synchro Out">
							<SignalLogicalOp Name="AND">
								<Signal Name="VP_$IpInstance_SYNCHRO_EXTERNAL"/>							
							</SignalLogicalOp>	
							<Semaphore>$IpInstance_SYNCHRO_EXTERNAL</Semaphore>	
							<!--<Condition Diagnostic="Need to have a mode selected" Expression="($IpInstance_SAIA_Mode | $IpInstance_SAIB_Mode) &amp; (!$IpInstance_SAIA_SPDIF) &amp; (!$IpInstance_PDM)"/>-->
							<!-- Bug 60846 -->
							<Condition Diagnostic="Need to have a mode selected" Expression="($IpInstance_SAIA_Master | $IpInstance_SAIA_AsyncSlave | $IpInstance_SAIB_Master | $IpInstance_SAIB_AsyncSlave)"/>
							<ContextCondition Expression="$IpInstance_CUBE" Diagnostic="Not available in this context"/>
						</Mode>							
					</ModeLogicOperator>					
				</Mode>	
			</ModeLogicOperator>
		</Mode>
	</ModeLogicOperator>
	
	
