	<!-- Modes Description for LPUART -->
	<ModeLogicOperator Name="OR">
            <Mode Name="Mode">
                <ModeLogicOperator Name="XOR">
                    <Mode Name="Asynchronous">
                        <SignalLogicalOp Name="AND">
                            <Signal IOMode="AlternateFunctionPushPullUp" Name="RX"/>
                            <Signal IOMode="AlternateFunctionPushPullUp" Name="TX"/>
                        </SignalLogicalOp>
                        <Semaphore>$IpInstance_Asynchronous</Semaphore>
                        <Semaphore>Asynchronous</Semaphore>
                    </Mode>
                    <Mode Name="Half_duplex(single_wire_mode)" UserName="Single Wire (Half-Duplex)">
                        <Condition Diagnostic="Not available when configuring IP from LPBAM context" Expression="!S_LPBAM_CONF"/>
                            <SignalLogicalOp Name="AND">
                                    <Signal IOMode="AlternateFunctionOpenDrain" Name="TX"/>
                            </SignalLogicalOp>
                    </Mode>
                    <Mode Name="Multiprocessor_communication" UserName="Multiprocessor Communication">
                        <Condition Diagnostic="Not available when configuring IP from LPBAM context" Expression="!S_LPBAM_CONF"/>
                        <SignalLogicalOp Name="AND">
                            <Signal Name="RX"/>
                            <Signal Name="TX"/>
                        </SignalLogicalOp>
                    </Mode>			
                </ModeLogicOperator> 
                <Semaphore>S_LPUART</Semaphore>
            </Mode>
            <Mode Name="Hardware Flow Control (RS232)">
                <ModeLogicOperator Name="XOR">
                    <Mode Name="CTS_Only" UserName="CTS Only">
                        <SignalLogicalOp Name="AND">
                            <Signal Name="CTS"/>
                        </SignalLogicalOp>
                        <Semaphore>$IpInstance_Clear</Semaphore>
                    </Mode>
                    <Mode Name="RTS_Only" UserName="RTS Only">
                        <SignalLogicalOp Name="AND">
                            <Signal Name="RTS"/>
                        </SignalLogicalOp>
                        <Semaphore>$IpInstance_Request</Semaphore>
                    </Mode>
                    <Mode Name="CTS_RTS" UserName="CTS/RTS">
                        <SignalLogicalOp Name="AND">
                            <Signal Name="CTS"/>
                            <Signal Name="RTS"/>
                        </SignalLogicalOp>
                        <Semaphore>$IpInstance_CTS_RTS</Semaphore>
                    </Mode>
                </ModeLogicOperator>
                <Condition Diagnostic="Active only with Asynchronous Mode" Expression="$IpInstance_Asynchronous &amp; !$IpInstance_HardwareControl &amp; !S_LPBAM_CONF"/>
                <Semaphore>$IpInstance_FlowControl</Semaphore>
                <Semaphore>S_LPUART</Semaphore>
            </Mode>
            <Mode Name="Hardware Flow Control (RS485)">
                <ModeLogicOperator Name="XOR">
                    <Mode Name="Hardware Flow Control (RS485)">
                        <SignalLogicalOp Name="AND">
                            <Signal Name="DE"/>
                        </SignalLogicalOp>
                    </Mode>
                </ModeLogicOperator>
                <Condition Diagnostic="Active only with Asynchronous Mode" Expression="!$IpInstance_FlowControl &amp; $IpInstance_Asynchronous &amp; !S_LPBAM_CONF"/>
                <Semaphore>$IpInstance_HardwareControl</Semaphore>
                <Semaphore>S_LPUART</Semaphore>
            </Mode>

<!-- LPBAM specific config -->
            <Mode Name="LPBAMSPECIFICTOP" UserName="LPBAM Advanced Settings" RemoveDisable="true" RemoveCondition="!($Index=1)|STM32WBA"> <!-- only LPUART1 is managed by LPBAM-->
                <Condition Diagnostic="only available when configurring IP from LPBAM context" Expression="S_LPBAM_CONF &amp; (S_LPUART)"/>
                <ModeLogicOperator Name="XOR">
                    <Mode Name="LPBAMSPECIFIC" UserName="">
                        <SignalLogicalOp Name="AND">
                            <Signal Name="VS_LPUART_LPBAM"/>
                        </SignalLogicalOp>
                    </Mode>
                </ModeLogicOperator>
            </Mode>
 <!-- LPBAM specific config -->
	</ModeLogicOperator>
        
	<!-- RefSignal for LPUART -->
	<RefSignal IOMode="AlternateFunctionPushPull" Name="CTS"/>
	<RefSignal IOMode="AlternateFunctionPushPull" Name="RTS"/>
	<RefSignal IOMode="AlternateFunctionPushPull" Name="DE"/>
	<RefSignal IOMode="AlternateFunctionPushPullUp" Name="RX"/>
	<RefSignal IOMode="AlternateFunctionPushPullUp" Name="TX"/>
	<RefSignal Name="VS_LPUART_LPBAM" Virtual="true"/> <!-- LPBAM specific config -->

	<Semaphore>LPUARTUsed_ForRCC</Semaphore>    
