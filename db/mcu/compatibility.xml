        <MXDBVersion>DB.4.0.190</MXDBVersion>
        <MatcherOnKey>(.*).WordLength</MatcherOnKey>
        <MatcherPattern>(.*)_WORDLENGTH_9B</MatcherPattern>
        <MatchToPattern>WORDLENGTH_9B</MatchToPattern>
        <SeparatorOnValue>__No_Separator</SeparatorOnValue>           
    </description>
</rule>
<rule name="Rename FMC NAND NCE signal">
    <description>
        <MXDBVersion>DB.4.0.180</MXDBVersion>
        <ParserCondition>(STM32F4&amp;DIE434)|(STM32F7&amp;(DIE449|DIE451))|(STM32H7&amp;DIE450)</ParserCondition>
        <MatcherOnKey>.*\.Signal</MatcherOnKey>
        <MatcherPattern>FMC_NCE3</MatcherPattern>
        <MatchToPattern>FMC_NCE</MatchToPattern>
        <SeparatorOnValue>,</SeparatorOnValue>
    </description>
</rule>
<rule name="Rename FMC NAND INT signal">
