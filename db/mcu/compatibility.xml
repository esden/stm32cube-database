<rule name="Add RTC Activate Clock Source">
    <description>
        <MXDBVersion>DB.4.0.170</MXDBVersion>
        <RuleType>AddIPMode</RuleType>
        <MatcherOnKey>Mcu.IP([0-9]|[0-9][0-9])</MatcherOnKey>
        <MatcherPattern>RTC</MatcherPattern>
        <MatchToPattern>Pin:VP_RTC_VS_RTC_Activate;AddedKeys:VP_RTC_VS_RTC_Activate.Mode=RTC_Enabled,VP_RTC_VS_RTC_Activate.Signal=RTC_VS_RTC_Activate</MatchToPattern>   
        <SeparatorOnValue>__No_Separator</SeparatorOnValue> 
     </description>
</rule>

<rule name="Add RTC Activate Calendar 1">
    <description>
        <MXDBVersion>DB.4.0.170</MXDBVersion>
        <RuleType>AddIPMode</RuleType>
        <MatcherOnKey>VP_RTC_VS_RTC_(.*).Mode</MatcherOnKey>
        <MatcherPattern>Alarm A|Alarm B|WakeUp</MatcherPattern>
        <MatchToPattern>Pin:VP_RTC_VS_RTC_Calendar;AddedKeys:VP_RTC_VS_RTC_Calendar.Mode=RTC_Calendar,VP_RTC_VS_RTC_Calendar.Signal=RTC_VS_RTC_Calendar</MatchToPattern>   
        <SeparatorOnValue>__No_Separator</SeparatorOnValue> 
    </description>
</rule>

<rule name="Add RTC Activate Calendar 2">
    <description>
        <MXDBVersion>DB.4.0.170</MXDBVersion>
        <RuleType>AddIPMode</RuleType>
        <MatcherOnKey>(.*).Mode</MatcherOnKey>
        <MatcherPattern>AlarmA_RoutedAF|AlarmB_RoutedAF|WakeUpAF1</MatcherPattern>
        <MatchToPattern>Pin:VP_RTC_VS_RTC_Calendar;AddedKeys:VP_RTC_VS_RTC_Calendar.Mode=RTC_Calendar,VP_RTC_VS_RTC_Calendar.Signal=RTC_VS_RTC_Calendar</MatchToPattern>   
        <SeparatorOnValue>__No_Separator</SeparatorOnValue> 
    </description>
</rule>

<rule name="Add RTC Activate Calendar 3">
    <description>
        <MXDBVersion>DB.4.0.170</MXDBVersion>
        <RuleType>AddIPMode</RuleType>
        <MatcherOnKey>(.*).Mode</MatcherOnKey>
        <MatcherPattern>Timestamp enabled - Input Enabled to AF1|Timestamp enabled - Input Enabled to AF2|Timestamp enabled</MatcherPattern>
        <MatchToPattern>Pin:VP_RTC_VS_RTC_Calendar;AddedKeys:VP_RTC_VS_RTC_Calendar.Mode=RTC_Calendar,VP_RTC_VS_RTC_Calendar.Signal=RTC_VS_RTC_Calendar</MatchToPattern>   
        <SeparatorOnValue>__No_Separator</SeparatorOnValue> 
    </description>
</rule>

