        <File Category="header" Name="Drivers/STM32WB0x_HAL_Driver/Inc/stm32wb0x_ll_bus.h"/>
        <File Category="header" Name="Drivers/STM32WB0x_HAL_Driver/Inc/stm32wb0x_ll_cortex.h"/>
