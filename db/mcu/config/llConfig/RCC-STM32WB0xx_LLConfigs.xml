        <File Category="header" Name="Drivers/STM32WB0x_HAL_Driver/Inc/stm32wb0x_ll_pwr.h"/>
        <File Category="header" Name="Drivers/STM32WB0x_HAL_Driver/Inc/stm32wb0x_ll_gpio.h"/>
