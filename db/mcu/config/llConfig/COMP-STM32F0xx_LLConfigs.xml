                   <MethodArg Name="COMPx" ParameterName="Instance"/>
                </CallLibMethod>
           </MethodArg> 
           <MethodArg Name="WindowMode" ParameterName="WindowMode"/>
        </CallLibMethod>
       </IFCondition>
