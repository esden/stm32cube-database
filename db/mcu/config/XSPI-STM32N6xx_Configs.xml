    <RefConfig Name="XSPI_Init">
        <CallLibMethod Name="HAL_XSPI_Init">
        </CallLibMethod>
        <ImplementCallBack Name="HAL_XSPI_MspInit"/> 
        <ImplementCallBack Name="HAL_XSPI_MspDeInit"/>
    </RefConfig>

    <!--IO Manager Configuration-->
    <RefConfig Name="IOManager_Config">
        <CallLibMethod Name="HAL_XSPIM_Config">
                <MethodArg Name="XSPIM_Cfg_Struct" ParameterName="sXspiManagerCfg"/>
        </CallLibMethod>        
    </RefConfig>
    
    <RefConfig  Name="HyperBus">
        <IFCondition Expression="MemoryType=HAL_XSPI_MEMTYPE_HYPERBUS">          
	<CallLibMethod Name="HAL_XSPI_HyperbusCfg">  
                <MethodArg Name="cfg" ParameterName="sHyperBusCfg"/>
        </CallLibMethod>        
        </IFCondition>
    </RefConfig>
    
    <RefConfig  Name="Auto_Polling">
        <IFCondition Expression="(OperationType = HAL_XSPI_OPTYPE_COMMON_CFG)">
        <CallLibMethod Name="HAL_XSPI_AutoPolling">
                <MethodArg Name="cfg" ParameterName="sAutoPollingCfg"/>
        </CallLibMethod>        
        <CallLibMethod Name="HAL_XSPI_AutoPolling_IT">
                <MethodArg Name="cfg" ParameterName="sAutoPollingCfg"/>
        </CallLibMethod>        
        </IFCondition>	
    </RefConfig>
    
    <RefConfig  Name="Memory_mapped">
        <IFCondition Expression="(OperationType = HAL_XSPI_OPTYPE_READ_CFG) | (OperationType = HAL_XSPI_OPTYPE_WRITE_CFG) | (OperationType = HAL_XSPI_OPTYPE_WRAP_CFG)">          
            <CallLibMethod Name="HAL_XSPI_MemoryMapped">             
                <MethodArg Name="cfg" ParameterName="sMemMappedCfg"/>
            </CallLibMethod>         
        </IFCondition>
    </RefConfig>
    
    <RefConfig Name="XSPI_DeInit">
        <CallLibMethod Name="HAL_XSPI_DeInit"/>
    </RefConfig>

