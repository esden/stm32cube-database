    <RefConfig Name="OSPI_Init">
        <CallLibMethod Name="HAL_OSPI_Init">
        </CallLibMethod>
        <ImplementCallBack Name="HAL_OSPI_MspInit"/> 
        <ImplementCallBack Name="HAL_OSPI_MspDeInit"/>
    </RefConfig>

    <!--IO Manager Configuration-->
    <RefConfig Name="IOManager_Config">
        <CallLibMethod Name="HAL_OSPIM_Config">
                <MethodArg Name="OSPIM_Cfg_Struct" ParameterName="sOspiManagerCfg"/>
        </CallLibMethod>        
    </RefConfig>
    
    <RefConfig  Name="HyperBus">
        <IFCondition Expression="MemoryType=HAL_OSPI_MEMTYPE_HYPERBUS">          
	<CallLibMethod Name="HAL_OSPI_HyperbusCfg">  
                <MethodArg Name="cfg" ParameterName="sHyperBusCfg"/>
        </CallLibMethod>        
        </IFCondition>
    </RefConfig>
    
    <RefConfig  Name="Auto_Polling">
        <IFCondition Expression="(OperationType = HAL_OSPI_OPTYPE_COMMON_CFG)">
        <CallLibMethod Name="HAL_OSPI_AutoPolling">
                <MethodArg Name="cfg" ParameterName="sAutoPollingCfg"/>
        </CallLibMethod>        
        <CallLibMethod Name="HAL_OSPI_AutoPolling_IT">
                <MethodArg Name="cfg" ParameterName="sAutoPollingCfg"/>
        </CallLibMethod>        
        </IFCondition>	
    </RefConfig>
  
    <!-- <RefConfig  Name="Command_Config">
        <CallLibMethod Name="HAL_OSPI_Command_IT">
        </CallLibMethod>	
    </RefConfig>-->
    
    <RefConfig  Name="Memory_mapped">
