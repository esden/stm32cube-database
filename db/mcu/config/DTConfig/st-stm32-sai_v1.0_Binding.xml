<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<BindingsList
	xmlns="http://mcd.rou.st.com/modules.php?name=mcu" xmlns:ns0="http://www.w3.org/2001/XMLSchema-instance" ns0:schemaLocation="" >

	<!--SAI binding-->
	<!--/////////////-->
	<Binding Name="st-stm32-sai" Specifications="Documentation/bindings/sound/st,stm32-sai.txt" >

		<Node>
			<Node Condition="$IpInstance_SAIA_Mode" Label="" Name="saia" UnitAddress="">
				<PinCtrl NodeNameMatcher="sai.+a"/>
			</Node>
					
			<Node  Condition="$IpInstance_SAIB_Mode" Label="" Name = "saib" UnitAddress="">
				<PinCtrl NodeNameMatcher="sai.+b"/>
			</Node>
		</Node>
		
	</Binding>

</BindingsList>

