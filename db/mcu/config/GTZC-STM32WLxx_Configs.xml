      </IFCondition>
      <IFCondition Expression="(S_IAI_ENABLED | S_IAI_DEFAULT) &amp; (AES_Illegal_Access_Interrupt=Disabled)">
        <CallLibMethod Name="HAL_GTZC_TZIC_DisableIT">
