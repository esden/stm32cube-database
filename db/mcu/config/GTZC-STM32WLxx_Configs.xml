      <IFCondition Expression="(S_IAI_DISABLED | S_IAI_DEFAULT) &amp; (RNG_Illegal_Access_Interrupt=Enabled)">
        <CallLibMethod Name="HAL_GTZC_TZIC_EnableIT">
