				remote-endpoint = <&dfsdm_endpoint1>;
			};
		};
	};

	dmic2:dmic-2{
		compatible = "dmic-codec";
		#sound-dai-cells = <1>;
		sound-name-prefix = "dmic2";
		status = "okay";

