		compatible = "dmic-codec";
		#sound-dai-cells = <1>;
		sound-name-prefix = "dmic0";
		status = "okay";

		port{

			dmic0_endpoint:endpoint{
				remote-endpoint = <&dfsdm_endpoint0>;
			};
		};
	};

	dmic1:dmic-1{
		compatible = "dmic-codec";
		#sound-dai-cells = <1>;
		sound-name-prefix = "dmic1";
		status = "okay";

		port{

