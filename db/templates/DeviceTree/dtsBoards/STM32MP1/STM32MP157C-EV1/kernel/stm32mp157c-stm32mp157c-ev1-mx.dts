		label = "STM32MP15-EV";
		routing = "AIF1CLK", "MCLK1",
				"AIF2CLK", "MCLK1",
				"IN1LN", "MICBIAS2",
				"DMIC2DAT", "MICBIAS1",
				"DMIC1DAT", "MICBIAS1";
