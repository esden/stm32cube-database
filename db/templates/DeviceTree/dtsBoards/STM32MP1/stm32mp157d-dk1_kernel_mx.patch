&pinctrl{
	stusb1600_pins_a:stusb1600-0{

		pins{
			bias-pull-up;
			pinmux = <STM32_PINMUX('I', 11, GPIO)>;
		};
	};
};

&usbh_ohci{
	phys = <&usbphyc_port0>;
};

/*override m4_rproc : <&ipcc 3> and "detach" not managed by MX */
&m4_rproc{
	mboxes = <&ipcc 0>, <&ipcc 1>, <&ipcc 2>, <&ipcc 3>;
	mbox-names = "vq0", "vq1", "shutdown","detach";
};